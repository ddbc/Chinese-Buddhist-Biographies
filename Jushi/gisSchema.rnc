    empty,
    att.global.attributes,
    att.pointing.attributes,
    att.declaring.attributes,
    (
     ## 提供一個或多個統一資源識別符參照 (URI References)
     ##                 來具體說明指標所指的位置。
     attribute target {
       list { xsd:anyURI+ }
     }
     | 
       ##  (canonical reference) 用元素refsDecl在TEI標頭內所定義的標準參照來說明指標所指位置。
       attribute cRef {
         list {
           xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
         }
       }),
    empty
  }
ref =
  
  ## 標明與其他位置互相參照的符號，或許包含附加的文字或註解。
  element ref {
    macro.paraContent,
    att.global.attributes,
    att.pointing.attributes,
    att.declaring.attributes,
    (
     ## 用一個或多個統一資源識別符參照 (URI References) 來說明參照所指位置。
     attribute target {
       list { xsd:anyURI+ }
     }?
     | 
       ##  (canonical reference) 用元素refsDecl在TEI標頭內所定義的標準參照來說明參照所指位置。
       attribute cRef {
         list {
           xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
         }
       }?),
    empty
  }
\list =
  
  ##  (list) 包含以列表方式呈現的任何連續項目。
  element list {
    ((model.divTop | model.global)*,
     ((item, model.global*)+
      | (label, model.global*, item, model.global*)+),
     (model.divBottom, model.global*)*),
    att.global.attributes,
    
    ## 描述列表的形式。
    ## Suggested values include: 1] ordered; 2] bulleted; 3] simple; 4] gloss
    [ a:defaultValue = "simple" ]
    attribute type {
      
      ## 列表項目以數字或字母依順序排列。
      "ordered"
      | 
        ## 列表項目以黑點或其他印刷符號排列。
        "bulleted"
      | 
        ## 列表項目的排列沒有用數字或任何其他符號作標記。
        "simple"
      | 
        ## 各列表項目為一些字詞或概念下註解，每個項目之前並帶有標籤元素。
        "gloss"
