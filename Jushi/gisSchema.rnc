variantEncoding =
  
  ## 宣告在標記不同版本中的變異字體時所使用的方法。
  element variantEncoding {
    empty,
    att.global.attributes,
    
    ## 指明標記變異的學術編輯註解時所使用的方法。
    attribute method {
      
      ## 學術編輯註解使用基礎文件中被參照的行數或其他標準參照架構。
      "location-referenced"
      | 
        ## 學術編輯註解指出與基礎文件相關的各主題起始與結束的確切位置
        "double-end-point"
      | 
        ## 文件中一個段落的多個替代讀本以平行方式呈現；不需要基礎文件
        "parallel-segmentation"
    },
    
    ## 指出該學術編輯註解出現在連續文字內部或其外部。
    attribute location {
      
      ## 學術編輯註解出現在連續文字內部
      "internal"
      | 
        ## 學術編輯註解出現在基礎文件外部
        "external"
    },
    empty
  }
model.rdgPart = wit | witStart | witEnd | lacunaStart | lacunaEnd
att.rdgPart.attributes = att.rdgPart.attribute.wit, empty
att.rdgPart.attribute.wit =
  
  ##  (witness or witnesses) 一個或多個印記列表，指出於此處開始或結束的版本。
  attribute wit {
    list { xsd:anyURI+ }
  }?
att.textCritical.attributes =
  att.textCritical.attribute.wit,
  att.textCritical.attribute.type,
  att.textCritical.attribute.cause,
  att.textCritical.attribute.varSeq,
  att.textCritical.attribute.resp,
  att.textCritical.attribute.hand,
  empty
att.textCritical.attribute.wit =
  
  ##  (witness or witnesses) 包含一個或多個指標的一份列表，連結到已知對應本的版本。
  attribute wit {
    list { xsd:anyURI+ }
  }?
att.textCritical.attribute.type =
  
  ## 用合宜的分類法將對應本分類。
  ## Sample values include: 1] substantive; 2] orthographic
  attribute type { xsd:Name }?
att.textCritical.attribute.cause =
  
  ## 利用可能來源的任何適當分類法，將變異對應本的產生原因分類。
  ## Sample values include: 1] homeoteleuton; 2] homeoarchy; 3] paleographicConfusion; 4] haplography; 5] dittography; 6] falseEmendation
  attribute cause { xsd:Name }?
att.textCritical.attribute.varSeq =
  
  ##  (variant sequence) 當有理由假設任一主題的一系列變異對應本時，則提供一個數字，指出此對應本於連續系列中的位置。
  attribute varSeq { xsd:nonNegativeInteger }?
att.textCritical.attribute.resp =
  
  ##  (responsible party) 指出負責在版本中插入一特定對應本的編者。
  attribute resp { xsd:anyURI }?
att.textCritical.attribute.hand =
  
  ## 指出該版本中特定對應本的負責人。
  attribute hand { xsd:anyURI }?
link =
  
  ## 定義元素或段落之間的關連或超文字連結，其他元素無法將此連結類型作更詳細說明。
  element link {
    empty,
    att.global.attributes,
    att.pointing.attributes,
    
    ## 標明被連結元素或段落的識別符碼。
    attribute targets {
      list { xsd:anyURI, xsd:anyURI+ }
    },
    empty
  }
linkGrp =
  
  ## 定義一群關連或超文字連結的集合。
  element linkGrp {
    (link | ptr)+,
    att.global.attributes,
    att.pointing.group.attributes,
    empty
  }
ab =
  
  ##  (匿名區塊) 包含文件中任何隨機組合性層次的單元，匿名收容類似一個段落、但不包含段落語義的字詞或中間層元素。
  element ab {
    macro.paraContent,
    att.global.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    
    ## 標明區塊是否完全。
    [ a:defaultValue = "N" ]
    attribute part {
      
      ##  (yes) 區塊不完全
      "Y"
      | 
        ##  (no) 區塊完全，或完全程度不明
        "N"
      | 
        ##  (initial) 不完全區塊的起始部分
        "I"
      | 
        ##  (medial) 不完全區塊的中段部分
        "M"
      | 
        ##  (final) 不完全區塊的末端部分
        "F"
    }?,
    empty
  }
anchor =
  
  ##  (錨點) 在文件中某一位置附上識別符碼，無論該位置是否符合一個文字性元素。
  element anchor {
    empty, att.global.attributes, att.typed.attributes, empty
  }
seg =
  
  ##  (隨機分割) 包含文件中任何隨機字詞層次的單元 (包括其他分割元素)。
  element seg {
    macro.paraContent,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    empty
  }
when =
  
  ## 指出一個時間點，相對或絕對於同一時間進程標籤裡的其他元素。
  element when {
    empty,
    att.global.attributes,
    
    ## 提供一個絕對時間值。
    attribute absolute {
      xsd:date
      | xsd:gYear
      | xsd:gMonth
      | xsd:gDay
      | xsd:gYearMonth
      | xsd:gMonthDay
      | xsd:time
      | xsd:dateTime
    }?,
    
    ## 若未由父元素timeLine得到時間單位資訊，則在此指明屬性interval所使用的時間單位。
    ## Suggested values include: 1] d (daysjoursgiornidías); 2] h (hoursheuresorehoras); 3] min (minutesminutosminuti); 4] s (secondssecondessecondisegundos); 5] ms (millisecondsmillisecondesmillesimi di secondomilésimas de segundo)
    attribute unit {
      
      ##  (days) 
      "d"
      | 
        ##  (hours) 
        "h"
      | 
        ##  (minutes) 
        "min"
      | 
        ##  (seconds) 
        "s"
      | 
        ##  (milliseconds) 
        "ms"
      | xsd:Name
    }?,
    
    ## 指明時間區段中的數值部份
    attribute interval {
      xsd:float { minExclusive = "0" }
      | "unknown"
    }?,
    
    ## 指出一個參照點，用以確定現有元素when的時間，可於參照點的時間內加入區段而取得。
    attribute since { xsd:anyURI }?,
    empty
  }
timeline =
  
  ##  (時間進程) 提供一組整齊的時間順序點，可與口說文本的元素相連結，以建立該文本的時間組序。
  element timeline {
    when+,
    att.global.attributes,
    
    ## 標出時間進程的開頭，例如開始時間。
    attribute origin { xsd:anyURI },
    
    ## 指出符合時間進程屬性interval的屬性值或是符合其接續時間點的時間單位。
    ## Suggested values include: 1] d (daysjoursgiornidías); 2] h (hoursheuresorehoras); 3] min (minutesminutosminuti); 4] s (secondssecondessecondisegundos); 5] ms (millisecondsmillisecondesmillesimi di secondomilésimas de segundo)
    attribute unit {
      
      ##  (days) 
      "d"
      | 
        ##  (hours) 
        "h"
      | 
        ##  (minutes) 
        "min"
      | 
        ##  (seconds) 
        "s"
      | 
        ##  (milliseconds) 
        "ms"
      | xsd:Name
    }?,
    
    ## 指明時間區段中的數值部份
    attribute interval {
      xsd:float { minExclusive = "0" }
      | "regular"
      | "irregular"
    }?,
    empty
  }
join =
  
  ## 標明文本中可能的不完整分割，指向構成此不完整分割的可能不連接元素。
  element join {
    model.glossLike*,
    att.global.attributes,
    att.pointing.attributes,
    
    ## 標明與一個虛擬元素相連結的元素或段落識別符碼。
    attribute targets {
      list { xsd:anyURI, xsd:anyURI+ }
    },
    
    ## 指出一個推斷該元素集合所代表的元素名稱。
    attribute result { xsd:Name }?,
    
    ## 指出被連結的目標是否包含所指元素的完整部份 (完整樹狀圖包含根節點) ，或僅包含該目標的子元素 (樹狀分支部份) 。
    [ a:defaultValue = "root" ]
    attribute scope {
      
      ## 由屬性target所指明、具根節點的樹狀圖彼此連結，每個樹狀圖成為該連結所產生之虛擬元素的子元素。
      "root"
      | 
        ## 由屬性target所指明的樹狀圖的子元素成為虛擬元素的子元素 (例如捨去樹狀圖的根節點) 。
        "branches"
    }?,
    empty
  }
joinGrp =
  
  ##  (連結群組) 匯集一群連結元素與可能指標。
  element joinGrp {
    (model.glossLike*, (join | ptr)+),
    att.global.attributes,
    att.pointing.group.attributes,
    
    ## 描述此集合中匯集的連結結果。
    attribute result { xsd:Name }?,
    empty
  }
att.global.linking.attributes =
  att.global.linking.attribute.corresp,
  att.global.linking.attribute.synch,
  att.global.linking.attribute.sameAs,
  att.global.linking.attribute.copyOf,
  att.global.linking.attribute.next,
  att.global.linking.attribute.prev,
  att.global.linking.attribute.exclude,
  att.global.linking.attribute.select,
  empty
att.global.linking.attribute.corresp =
  
  ##  (corresponds) 連結到的元素在某方面符合現有元素。
  attribute corresp {
    list { xsd:anyURI+ }
  }?
att.global.linking.attribute.synch =
  
  ##  (synchronous) 連結到的元素和現有元素同時出現。
  attribute synch {
    list { xsd:anyURI+ }
  }?
att.global.linking.attribute.sameAs =
  
  ## 連結到的元素和現有元素相同。
  attribute sameAs { xsd:anyURI }?
att.global.linking.attribute.copyOf =
  
  ## 現有元素為所連結元素的複製本。
  attribute copyOf { xsd:anyURI }?
att.global.linking.attribute.next =
  
  ## 連結到現有元素所屬虛擬集合中的下一個元素。
  attribute next { xsd:anyURI }?
att.global.linking.attribute.prev =
  
  ##  (previous) 連結到現有元素所屬虛擬集合中的上一個元素。
  attribute prev { xsd:anyURI }?
att.global.linking.attribute.exclude =
  
  ## 連結到的元素是現有元素的專有替換元素。
  attribute exclude {
    list { xsd:anyURI+ }
  }?
att.global.linking.attribute.select =
  
  ## 選擇一個或多個替換；若選擇一個替換，則疑惑或不明確被標記為已解決。若選擇的替換多於一個，則疑惑或不明確的程度因為未選擇的替換而被標記為減低。
  attribute select {
    list { xsd:anyURI+ }
  }?
att.pointing.attributes =
  att.pointing.attribute.type, att.pointing.attribute.evaluate, empty
att.pointing.attribute.type =
  
  ## 用任何合宜的類別，在某方面將指標分類。
  attribute type { xsd:Name }?
att.pointing.attribute.evaluate =
  
  ## 若指標的目標本身為指標，則在此說明其用意。
  attribute evaluate {
    
    ## 若指向的元素本身為指標，則該指標的目標會被移除，依此類推，直到找到一個非指標的元素為止。
    "all"
    | 
      ## 若指向的元素本身為指標，則該指標的目標(無論是否為指標)會作為這項指標的目標。
      "one"
    | 
      ## 尋找指標的目標元素時，未對目標進行不必要的延伸評估。
      "none"
  }?
att.pointing.group.attributes =
  att.pointing.attributes,
  att.pointing.group.attribute.domains,
  att.pointing.group.attribute.targFunc,
  empty
att.pointing.group.attribute.domains =
  
  ## 標明元素的識別符碼 (但非必備) ，此元素內容所指明的元素皆包含於這些元素當中。
  attribute domains {
    list { xsd:Name+ }
  }?
att.pointing.group.attribute.targFunc =
  
  ##  (target function) 描述每個元素標籤link、join、或alt當中的屬性targets的屬性值功能。
  attribute targFunc {
    list {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
    }
  }?
start = TEI | teiCorpus
