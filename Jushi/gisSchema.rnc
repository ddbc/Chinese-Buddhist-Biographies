    att.global.attributes,
    att.naming.attributes,
    att.typed.attributes,
    empty
  }
rs =
  
  ##  (參照字串) 包含一般名稱或參照字串。
  element rs {
    macro.phraseSeq,
    att.global.attributes,
    att.naming.attributes,
    
    ## 更詳細指出參照字串所表示的對象。屬性值可包含人物、地點、船、元素等。
    attribute type { xsd:Name }?,
    empty
  }
email =
  
  ## 包含一個電子郵件位址，標明電子郵件訊息可傳送的位置。
  element email { macro.phraseSeq, att.global.attributes, empty }
date =
  
  ## 包含任何格式的日期表示。
  element date {
    (text | model.gLike | model.phrase | model.global)*,
    
    ## The unique identifier for this date as derived from the DDBC Date
    ##                                 Authority Database.
    attribute key { xsd:string }?,
    
    ## 指明該日期表示所使用的曆法計算系統。
    ## Suggested values include: 1] Gregorian; 2] Julian; 3] Islamic; 4] Hebrew; 5] Revolutionary; 6] Iranian; 7] Coptic; 8] Chinese
    attribute calendar {
      
      ## 格里高里曆
      "Gregorian"
      | 
        ## 凱撒曆
        "Julian"
      | 
        ## 伊斯蘭或穆斯林 (哈吉來) 陽曆
        "Islamic"
      | 
        ## 希伯來或猶太陰陽曆
        "Hebrew"
      | 
        ## 法國共和曆 
        "Revolutionary"
      | 
        ## 伊朗或波斯 (Jalaali) 陽曆
        "Iranian"
      | 
        ## 埃及古曆或亞歷山大曆
        "Coptic"
      | 
        ## 中國陰陽曆
        "Chinese"
      | xsd:Name
    }?,
    att.global.attribute.xmlid,
    att.global.attribute.n,
    att.global.attribute.xmllang,
    att.global.attribute.rend,
    att.global.attribute.rendition,
    att.global.attribute.xmlbase,
    att.global.linking.attributes,
    
    ##  (analysis) Allows a value restricted by a regEx.
    attribute ana {
      xsd:token { pattern = "#time" }
    }?,
    att.datable.attributes,
    att.editLike.attributes,
    att.typed.attributes,
    empty
  }
