bibl =
  
  ##  (書目資料) 包含結構零散的書目資料，其中次要元件不一定會明確標記。
  element bibl {
    (text
     | model.gLike
     | model.highlighted
     | model.pPart.data
     | model.pPart.edit
     | model.segLike
     | model.ptrLike
     | model.biblPart
     | model.global)*,
    att.global.attributes,
    att.declarable.attributes,
    att.typed.attributes,
    empty
  }
biblStruct =
  
  ##  (結構次要書目) 包含僅出現子節點的結構書目資料，並以特定順序呈現。
  element biblStruct {
    (model.noteLike | idno | relatedItem)*,
    att.global.attributes,
    att.declarable.attributes,
    att.typed.attributes,
    empty
  }
listBibl =
  
  ##  (書目列表) 包含任何種類的書目資料列表。
  element listBibl {
    (model.headLike*,
     (model.biblLike | model.milestoneLike | listBibl)+),
    att.global.attributes,
    att.declarable.attributes,
    att.typed.attributes,
    empty
  }
relatedItem =
  
  ## 包含或參照到其他書目項目，該些項目與目前的書目項目在某種程度上相關，例如作為其組成或者替代項目。
  element relatedItem {
    (model.biblLike | model.ptrLike),
    att.global.attributes,
    att.typed.attributes,
    empty
  }
l =
  
  ##  (詩行) 包含詩文的一行，也許是不完整的詩行。
  element l {
    macro.paraContent,
    att.global.attributes,
    
    ## 說明詩行的韻律表現是否完整。
    [ a:defaultValue = "N" ]
    attribute part {
      
      ##  (yes) 韻律表現不完整的詩行
      "Y"
      | 
        ##  (no) 詩行完整或未說明其完整性
        "N"
      | 
        ##  (initial) 不完整詩行的起始部分
        "I"
      | 
        ##  (medial) 不完整詩行的中段部分
        "M"
      | 
        ##  (final) 不完整詩行的末端部分
        "F"
    }?,
    empty
  }
lg =
  
  ##  (行組) 包含形式上視為一組的詩行，例如詩節、疊句、韻文段落等。 
  element lg {
    ((model.divTop | model.global)*,
     (model.lLike | lg),
     (model.lLike | lg | model.global)*,
     (model.divBottom, model.global*)*),
    att.global.attributes,
    att.divLike.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    empty
  }
teiCorpus =
  
  ## 包含一套用TEI編碼的文件集，由單一文集標頭以及一個 (或多個)
  ##         TEI元素所組成，各TEI元素包含單一文本標頭和一個文本。
  element teiCorpus {
    (teiHeader, (TEI | teiCorpus)+),
    att.global.attributes,
    
    ## TEI架構的版本
    [ a:defaultValue = "5.0" ] attribute version { xsd:decimal }?,
    empty
  }
model.headerPart = encodingDesc | profileDesc
teiHeader =
  
  ##  (TEI標頭) 在所有符合TEI標準的文本起始的電子題名頁當中提供敘述性以及宣告性的資訊。
  element teiHeader {
    (fileDesc, model.headerPart*, revisionDesc?),
    att.global.attributes,
    
    ## 標明標頭所附屬於的文件類型，例如文集或單一文本。
    ## Sample values include: 1] text; 2] corpus
    [ a:defaultValue = "text" ] attribute type { xsd:Name }?,
    empty
  }
fileDesc =
  
  ##  (檔案描述) 包含對電子檔案的完整書目敘述。
  element fileDesc {
    ((titleStmt,
      editionStmt?,
      extent?,
      publicationStmt,
      seriesStmt?,
      notesStmt?),
     sourceDesc+),
    att.global.attributes,
    empty
  }
titleStmt =
  
  ##  (題名陳述) 匯集關於作品標題與作品智慧內容責任歸屬的資訊。
  element titleStmt {
    (title+, model.respLike*), att.global.attributes, empty
  }
sponsor =
  
  ## 標明贊助的組織或機構名稱。
  element sponsor {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
funder =
  
  ##  (資助者) 標明負責資助一項計畫或文件製作的個人、機構或組織名稱。
  element funder {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
principal =
  
  ##  (主導研究者) 提供負責電子文件製作的主導研究者的姓名。
  element principal {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
editionStmt =
  
  ##  (版本陳述) 匯集文件某一版本的相關資訊。
  element editionStmt {
    (model.pLike+ | (edition, respStmt*)),
    att.global.attributes,
    empty
  }
edition =
  
  ##  (版本) 描述文件某一版本的特質。
  element edition { macro.phraseSeq, att.global.attributes, empty }
extent =
  
  ## 描述電子文件儲存在某一承載媒介時的約略大小，標以任何適用的單位。
  element extent { macro.phraseSeq, att.global.attributes, empty }
publicationStmt =
  
  ##  (出版陳述) 匯集電子文件或其他類型文件的出版或發行相關資訊。
  element publicationStmt {
    (model.pLike+ | model.publicationStmtPart+),
    att.global.attributes,
    empty
  }
distributor =
  
  ## 提供負責發行文件的個人或其他經銷商的名稱。
  element distributor { macro.phraseSeq, att.global.attributes, empty }
authority =
  
  ##  (供應機構) 說明除了出版者或發行者之外，負責提供電子檔案的個人或其他經銷商的名稱。
  element authority {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
idno =
  
  ##  (識別代碼) 提供任何用來識別書目項目的標準或非標準編碼。
  element idno {
    text,
    att.global.attributes,
    
    ## 表明編碼的種類，例如國際標準書號 (ISBN) 或其他標準系列編碼。
    attribute type { xsd:Name }?,
    empty
  }
availability =
  
  ## 提供關於文件可利用性的資訊，例如使用或發行上的任何限制、版權狀態等。
  element availability {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ## 提供一個代號，用來確認文件目前的可利用性。
    [ a:defaultValue = "unknown" ]
    attribute status {
      
      ## 文件的可利用性不受限。
      "free"
      | 
        ## 文件狀態不明。
        "unknown"
      | 
        ## 文件的可利用性受限制。
        "restricted"
    }?,
    empty
  }
seriesStmt =
  
  ##  (集叢陳述) 若出版品隸屬於某叢書，則在此匯集該叢書的相關資訊。
  element seriesStmt {
    (model.pLike+
     | (title+, (idno | respStmt)*)),
    att.global.attributes,
    empty
  }
notesStmt =
  
  ##  (附註陳述) 收集其他部分的書目描述中所未記載的任何文件相關附註。
  element notesStmt { model.noteLike+, att.global.attributes, empty }
model.sourceDescPart = notAllowed
sourceDesc =
  
  ## 提供所製電子文件的一個或數個來源文件的書目描述。
  element sourceDesc {
    (model.pLike+
     | (model.biblLike | model.sourceDescPart | model.listLike)+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
biblFull =
  
  ## 包含結構完整的書目資料，其中呈現出TEI檔案描述內的所有元件。
  element biblFull {
    ((titleStmt,
      editionStmt?,
      extent?,
      publicationStmt,
      seriesStmt?,
      notesStmt?),
     sourceDesc*),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
model.encodingPart =
  projectDesc
  | samplingDecl
  | editorialDecl
  | tagsDecl
  | refsDecl
  | classDecl
  | appInfo
  | geoDecl
  | charDecl
  | variantEncoding
encodingDesc =
  
  ##  (編碼描述) 記錄電子文件和其ㄧ個或多個文件來源之間的關係。
  element encodingDesc {
    (model.encodingPart | model.pLike)+, att.global.attributes, empty
  }
projectDesc =
  
  ##  (計畫描述) 詳述將電子檔案編碼的目標或目的，以及其他關於檔案匯集或收集程序的任何資訊。
  element projectDesc {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
samplingDecl =
  
  ##  (取樣宣告) 以篇章描述的方式說明建立文集或文選時文件取樣的原理與方法。
  element samplingDecl {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
model.editorialDeclPart =
  correction
  | normalization
  | quotation
  | hyphenation
  | segmentation
  | stdVals
  | interpretation
editorialDecl =
  
  ##  (編輯實行宣告) 提供文件編碼時使用的編輯原則與實行方法的細節。
  element editorialDecl {
    (model.pLike+ | model.editorialDeclPart+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
correction =
  
  ##  (更正原則) 說明文本中的更正是在什麼情況下、如何產生的。
  element correction {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ## 指出文本更正的程度。
    [ a:defaultValue = "unknown" ]
    attribute status {
      
      ## 該文本以被徹底檢查校對過。
      "high"
      | 
        ## 該文本被檢查過至少一次。
        "medium"
      | 
        ## 該文本從未被檢查過。
        "low"
      | 
        ## 更正狀態不明
        "unknown"
    }?,
    
    ## 說明標示更正所採取的方法。
    [ a:defaultValue = "silent" ]
    attribute method {
      
      ## 直接在文本中更正
      "silent"
      | 
        ## corrections have been represented using markup
        "markup"
    }?,
    empty
  }
normalization =
  
  ## 指出來源文件轉換成電子文本時規格化或標準化的程度。
  element normalization {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ## 指出任何規格化所實行的依據。
    attribute source { xsd:anyURI }?,
    
    ## 指出標示規格化所採取的方法。
    [ a:defaultValue = "silent" ]
    attribute method {
      
      ## 直接在文本中實行規格化
      "silent"
      | 
        ## normalization represented using markup
        "markup"
    }?,
    empty
  }
quotation =
  
  ## 說明在編輯上是如何處理來源文件中的引文符號。
  element quotation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ##  (quotation marks) 指出引文符號是否在文本中被保留為內容的一部份。
    [ a:defaultValue = "all" ]
    attribute marks {
      
      ## 不保留任何引文符號
      "none"
      | 
        ## 保留部分引文符號
        "some"
      | 
        ## 保留所有引文符號
        "all"
    }?,
    
    ## 說明引文符號如何標示於文本中。
    [ a:defaultValue = "unknown" ] attribute form { text }?,
    empty
  }
hyphenation =
  
  ## 概述來源文件裡的連字符號在電子化的版本中是如何被處理的。
  element hyphenation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ##  (end-of-line) 指出行末連字符號是否在文本中被保留。
    [ a:defaultValue = "some" ]
    attribute eol {
      
      ## 保留所有行末連字符號，即使原文中的不被保留。
      "all"
      | 
        ## 行末連字符號在某些情況下被保留。
        "some"
      | 
        ## 所有軟音的行末連字符號皆被移除：任何剩下的行末連字符號應該被保留。
        "hard"
      | 
        ## 所有行末連字符號皆被移除：剩下的是行中出現的行末連字符號。
        "none"
    }?,
    empty
  }
segmentation =
  
  ## 描述文件分割的原理方法，例如分割成句子、音調單位、字形層次等。
  element segmentation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
stdVals =
  
  ##  (標準值) 明確說明文本中標準化日期或數值所使用的格式。
  element stdVals {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
interpretation =
  
  ## 指出除了轉錄之外，文件中加入哪些分析性或詮釋性的資訊。
  element interpretation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
tagsDecl =
  
  ##  (標誌宣告) 提供關於應用在XML文件中的標誌的詳細資訊。
  element tagsDecl {
    (rendition*, \namespace*), att.global.attributes, empty
  }
tagUsage =
  
  ## 提供文件中某個特定元素的使用資訊。
  element tagUsage {
    macro.limitedContent,
    att.global.attributes,
    
    ##  (element name) 該標籤所指明的元素名稱 (通用識別符碼)。
    attribute gi { xsd:Name },
    
    ## 指出該元素在文本中出現的次數。
    attribute occurs { xsd:nonNegativeInteger }?,
    
    ##  (with unique identifier) 指出帶有明確全域屬性xml:id屬性值的元素在文本中的出現次數。
    attribute withId { xsd:nonNegativeInteger }?,
    
    ## 標明元素rendition的識別符號，該符號定義此元素的呈現方法。
    attribute render { xsd:anyURI }?,
    empty
  }
\namespace =
  
  ## 提供其子元素所描述的元素所屬的名稱空間之正式名稱。 
  element namespace {
    tagUsage+,
    att.global.attributes,
    
    ## 名稱空間的正式完整名稱。
    attribute name { xsd:anyURI },
    empty
  }
rendition =
  
  ## 提供關於一個或多個元素預定呈現方式的資訊。
  element rendition {
    macro.limitedContent,
    att.global.attributes,
    
    ## identifies the language used to describe the rendition.
    attribute scheme {
      
      ## Cascading Stylesheet Language
      "css"
      | 
        ## Extensible  Stylesheet Language
        ## Formatting Objects
        "xslfo"
      | 
        ## Informal free text description
        "free"
      | 
        ## A user-defined rendition description language
        "other"
    }?,
    empty
  }
refsDecl =
  
  ##  (參照宣告) 標明本文件與某一個權威參照標準的關係。
  element refsDecl {
    (model.pLike+ | cRefPattern+ | refState+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
cRefPattern =
  
  ##  (定義如何將標準參照轉換成統一資源識別符 (URI) 。) 標明將標準參照轉換成統一資源識別符的表示方法與取代模式。 
  element cRefPattern {
    model.pLike*,
    att.global.attributes,
    
    ## 標明一個表示屬性cRef屬性值的固定方法。
    attribute matchPattern { xsd:token },
    
    ## 標明一個取代模式，一旦進行次要模式的替換，會自動提供統一資源識別符 (URI)  。
    attribute replacementPattern { text },
    empty
  }
refState =
  
  ##  (分界狀態) 標明分界方法所定義的權威參照標準裡其中一個組件。
  element refState {
    empty,
    att.global.attributes,
    att.sourced.attributes,
    
    ## indicates what kind of state is changing at this milestone.
    ## Suggested values include: 1] page; 2] column; 3] line; 4] book; 5] poem; 6] canto; 7] stanza; 8] act; 9] scene; 10] section; 11] absent
    attribute unit {
      
      ## page breaks in the reference edition.
      "page"
      | 
        ## column breaks.
        "column"
      | 
        ## line breaks.
        "line"
      | 
        ## any units termed book, liber, etc.
        "book"
      | 
        ## individual poems in a collection.
        "poem"
      | 
        ## cantos or other major sections of a poem.
        "canto"
      | 
        ## stanzas within a poem, book, or canto.
        "stanza"
      | 
        ## acts within a play.
        "act"
      | 
        ## scenes within a play or act.
        "scene"
      | 
        ## sections of any kind.
        "section"
      | 
        ## passages not present in the reference edition.
        "absent"
      | xsd:Name
    },
    
    ## specifies the fixed length of the reference component.
    attribute length { xsd:nonNegativeInteger }?,
    
    ##  (delimiter) supplies a delimiting string following the reference
    ##           component.
    attribute delim { text }?,
    empty
  }
classDecl =
  
  ##  (分類宣告) 包含一個或多個分類法，用來定義在文件中他處所使用之任何分類碼。
  element classDecl { taxonomy+, att.global.attributes, empty }
taxonomy =
  
  ## 定義文件分類的類型學，可以是潛在地以書目資料的方式，或是明確地以結構分類法的方式來分類。
  element taxonomy {
    (model.glossLike* | category+ | (model.biblLike, category*)),
    att.global.attributes,
    empty
  }
category =
  
  ## 在使用者定義之分類法當中，此元素包含一項個別的描述性類目，該類目可能位於一個更上層的類目之中。
  element category {
    ((catDesc | model.glossLike*), category*),
    att.global.attributes,
    empty
  }
catDesc =
  
  ##  (類目描述) 描述分類法中或文件類型學中的某些類目，可用短文描述的型式，或是用TEI正式元素textDesc所使用的狀況參數。
  element catDesc {
    (text | model.limitedPhrase | model.catDescPart)*,
    att.global.attributes,
    empty
  }
appInfo =
  
  ##  (application information) records information about an application which has
  ##   edited the TEI file.
  element appInfo {
    model.applicationLike+, att.global.attributes, empty
  }
application =
  
  ## provides information about an application which has acted upon the document.
  element application {
    (model.labelLike+, (model.ptrLike* | model.pLike*)),
    att.global.attributes,
    att.typed.attributes,
    att.datable.attributes,
    
    ## Supplies an identifier for the application, independent of its version number or display
    ##         name.
    attribute ident { xsd:Name },
    
    ## Supplies a version number for the application, independent of its identifier or display
    ##         name.
    attribute version {
      xsd:token {
        pattern = "[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}"
      }
    },
    empty
  }
model.applicationLike = application
model.profileDescPart = langUsage | textClass
profileDesc =
  
  ##  (文件背景描述) 提供文件非書目性方面的細節描述，特別是所使用的語言及次要語言、在何種情況下製作、參與人員及其環境背景。
  element profileDesc {
    (creation?, model.profileDescPart*), att.global.attributes, empty
  }
handNote =
  
  ##  (書寫特徵) 描述在手稿中可明顯辨識出的特殊風格或書寫手法。
  element handNote {
    macro.specialPara,
    att.global.attributes,
    att.handFeatures.attributes,
    empty
  }
creation =
  
  ## 包含關於文件建置的資訊。 
  element creation {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
langUsage =
  
  ##  (語言使用) 描述文件中使用的語言、次要語言、語域、方言等。
  element langUsage {
    language+, att.global.attributes, att.declarable.attributes, empty
  }
language =
  
  ## 個別敘述文本中使用的語言或次要語言。
  element language {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ##  (identifier) 提供一語言代碼，其編碼方式定義在RFC 3066中 (或其系列中) ，用來識別此元素所紀錄的語言，並由全域屬性xml:lang所參照。
    attribute ident { xsd:language },
    
    ## 標明該文本使用此語言的大約比例 (以冊計) 。
    attribute usage {
      xsd:nonNegativeInteger { maxInclusive = "100" }
    }?,
    empty
  }
textClass =
  
  ##  (文件分類) 匯集以標準分類架構、索引典等來描述文件性質或主題的資訊。
  element textClass {
    (classCode | catRef | keywords)*,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
keywords =
  
  ## 包含標明文件主題或性質的關鍵詞或字詞列表。
  element keywords {
    (term+ | \list),
    att.global.attributes,
    
    ## 標明相關關鍵詞所使用到的詞彙範圍。
    attribute scheme { xsd:anyURI },
    empty
  }
classCode =
  
  ##  (classification code) 包含文件在某標準分類系統中所屬的分類代碼。
  element classCode {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明使用中的分類系統或分類法。
    attribute scheme { xsd:anyURI },
    empty
  }
catRef =
  
  ##  (類目參照) 標明在某分類法或文件類型學中，一個或多個已定義之類目。
  element catRef {
    empty,
    att.global.attributes,
    
    ## 標明相關的類目。 
    attribute target {
      list { xsd:anyURI+ }
    },
    
    ## 標明該分類架構，其中相關的類目群組已被定義
    attribute scheme { xsd:anyURI }?,
    empty
  }
revisionDesc =
  
  ##  (修訂描述) 概述檔案的修訂歷史。
  element revisionDesc {
    (\list | change+), att.global.attributes, empty
  }
change =
  
  ## 概述多位研究者共享的某版本電子文本當中，某特定的內容變更或修正。
  element change {
    (text | model.limitedPhrase | model.inter | model.global)*,
    att.global.attributes,
    att.ascribed.attributes,
    
    ## supplies the date of the change in standard form, i.e. YYYY-MM-DD.
    attribute when {
      xsd:date
      | xsd:gYear
      | xsd:gMonth
      | xsd:gDay
      | xsd:gYearMonth
      | xsd:gMonthDay
      | xsd:time
      | xsd:dateTime
    }?,
    empty
  }
typeNote =
  
  ## describes a particular font or other significant typographic
  ##   feature distinguished within the description of a printed resource.
  element typeNote {
    macro.specialPara,
    att.global.attributes,
    att.handFeatures.attributes,
    empty
  }
geoDecl =
  
  ##  (geographic coordinates declaration) documents the notation and the datum used for geographic
  ##   coordinates expressed as content of the geo element
  ##   elsewhere within the document.
  element geoDecl {
    macro.phraseSeq,
    att.global.attributes,
    att.declarable.attributes,
    
    ## supplies a commonly used code name for the datum employed.
    ## Suggested values include: 1] WGS84 (World Geodetic System세계 측지 시스템Sistema Geodésico Mundialsistema geodetico mondiale); 2] MGRS (Military Grid Reference System군사 좌표 참조 시스템Sistema de Coordenadas MGRSsistema di riferimento delle coordinate MGRS); 3] OSGB36 (ordnance survey great britain영국 육지 측량부Ordnance Survey, Great Britain 1936sistema di riferimento a reticolato OSGB36); 4] ED50 (European Datum coordinate system유럽 자료 좌표 시스템Sistema  de coordinadas European Datumsistema di riferimento ED50)
    [ a:defaultValue = "WGS84" ]
    attribute datum {
      
      ##  (World Geodetic System) a pair of numbers to be interpreted as latitude followed by longitude
      ##	  according to the World Geodetic System.
      "WGS84"
      | 
        ##  (Military Grid Reference System) the values supplied are geospatial entity object codes,
        ##	based on  Universal Transverse Mercator coordinates
        "MGRS"
      | 
        ##  (ordnance survey great britain) the value supplied is to be interpreted as a British National Grid
        ##	  Reference.
        "OSGB36"
      | 
        ##  (European Datum coordinate system) the value supplied is to be interpreted as latitude followed by
        ##	  longitude according to the European Datum coordinate system.
        "ED50"
      | xsd:Name
    }?,
    empty
  }
TEI =
  
  ##  (TEI文件) 包含符合TEI標準的單一文件，由一個TEI標頭及一份文本組成，可單獨出現或是作為元素teiCorpus的一部分。
  element TEI {
    (teiHeader,
     ((model.resourceLike+, \text?) | \text))
    >> s:ns [ prefix = "tei" uri = "http://www.tei-c.org/ns/1.0" ]
    >> s:ns [
         prefix = "rng"
         uri = "http://relaxng.org/ns/structure/1.0"
       ],
    att.global.attributes,
    
    ## TEI架構的版本
    [ a:defaultValue = "5.0" ] attribute version { xsd:decimal }?,
    empty
  }
\text =
  
  ## 包含一份任何種類的文本，無論是單一或複合的，例如詩詞或戲劇、散文集、小說、字典、或是文集範例。
  element text {
    (model.global*,
     (front, model.global*)?,
     (body | group),
     model.global*,
     (back, model.global*)?),
    att.global.attributes,
    att.declaring.attributes,
    att.typed.attributes,
    empty
  }
body =
  
  ##  (正文) 單篇文章的整體部分，不包含正文前及正文後資訊。
  element body {
    (model.global*,
     (model.divTop, (model.global | model.divTop)*)?,
     (model.divGenLike, (model.global | model.divGenLike)*)?,
     ((model.divLike, (model.global | model.divGenLike)*)+
      | (model.div1Like, (model.global | model.divGenLike)*)+
      | ((model.common, model.global*)+,
         ((model.divLike, (model.global | model.divGenLike)*)+
          | (model.div1Like, (model.global | model.divGenLike)*)+)?)),
     (model.divBottom, model.global*)*),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
group =
  
  ## 包含複合文本的正文，所匯集的一連串單一文件 (或文件群組)
  ##     ，因為某特殊目的被視為一個整體，例如一個作者的作品集合、一連串的散文集等。
  element group {
    ((model.divTop | model.global)*,
     ((\text | group), (\text | group | model.global)*),
     model.divBottom*),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
\div =
  
  ##  (文本區段) 文本的正文前資訊、正文、及正文後資訊的分段。
  element div {
    ((model.divTop | model.global)*,
     ((((model.divLike | model.divGenLike), model.global*)+
       | ((model.common, model.global*)+,
          ((model.divLike | model.divGenLike), model.global*)*)),
      (model.divBottom, model.global*)*)?),
    att.global.attributes,
    att.divLike.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    empty
  }
postscript =
  
  ## contains a postscript, e.g. to a letter.
  element postscript {
    (model.common | model.global)*, att.global.attributes, empty
  }
front =
  
  ##  (正文前資訊) 包含位於文件最前端、正文之前的項目 (標頭、題名頁、前言、獻詞等) 。 
  element front {
    ((model.frontPart | model.pLike.front | model.global)*,
     ((model.div1Like,
       (model.frontPart | model.div1Like | model.global)*)
      | (model.divLike,
         (model.frontPart | model.divLike | model.global)*))?,
     (model.divBottomPart, (model.divBottomPart | model.global)*)?),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
back =
  
  ##  (正文後資訊) 在正文之後，包含附錄等。
  element back {
    ((model.frontPart | model.pLike.front | model.global)*,
     ((model.div1Like,
       (model.frontPart | model.div1Like | model.global)*)
      | (model.divLike,
         (model.frontPart | model.divLike | model.global)*))?,
     (model.divBottomPart, (model.divBottomPart | model.global)*)?),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
s =
  
  ##  (句子單元) 包含文字中一個句子組成的區段。
  element s {
    macro.phraseSeq,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    empty
  }
w =
  
  ##  (單字) 表示文法上 (但未必是拼字法上) 定義的單字。
  element w {
    (text
     | model.gLike
     | model.segLike
     | model.global
     | model.lPart
     | model.hiLike
     | model.pPart.edit)*,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    
    ## 指出在字典中該字的詞條形式。
    attribute lemma { xsd:string }?,
    
    ## provides a pointer to a definition for the root form of this word
    ## form.
    attribute lemmaRef { xsd:anyURI }?,
    empty
  }
c =
  
  ##  (文字符號) 表示一個文字符號。
  element c {
    macro.xtext,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    empty
  }
att.global.analytic.attributes =
  att.global.analytic.attribute.ana, empty
att.global.analytic.attribute.ana =
  
  ##  (analysis) 指出一個或多個元素，其中包含帶有屬性ana的元素解釋。
  attribute ana {
    list { xsd:anyURI+ }
  }?
span =
  
  ## 將詮釋性註釋直接和一段文字段連結。
  element span {
    macro.phraseSeq.limited,
    att.global.attributes,
    att.interpLike.attributes,
    
    ## 指出被註解段落的起點；若沒有使用屬性to，則指出全部段落。
    attribute from { xsd:anyURI },
    
    ## 指出被註解段落的終點。
    attribute to { xsd:anyURI }?,
    empty
  }
spanGrp =
  
  ##  (文字段群組) 匯集文字段標籤。
  element spanGrp {
    span*, att.global.attributes, att.interpLike.attributes, empty
  }
interp =
  
  ##  (解釋) 標明和某一文字段相連結的特定解釋性註釋。
  element interp {
    (text | model.gLike | model.glossLike)*,
    att.global.attributes,
    att.interpLike.attributes,
    empty
  }
interpGrp =
  
  ##  (解釋群組) 匯集一系列具有共同任務或類型的相關解釋。
  element interpGrp {
    (model.glossLike*, interp+),
    att.global.attributes,
    att.interpLike.attributes,
    empty
  }
certainty =
  
  ## 指出標記文本某些部分的確定或不確定的程度。
  element certainty {
    model.glossLike*,
    att.global.attributes,
    
    ## 指出不確定標記的元素。
    attribute target {
      list { xsd:anyURI+ }
    },
    
    ## 指出不確定標記的確切位置：元素的應用性、開始標籤或結束標籤的確切位置、特定屬性的屬性值等。
    ## Suggested values include: 1] gi (element name요소명Il n'est pas certain que l'élément utilisé
    ##               s'applique vraiment au passagenome di elementoes incierto si el elemento usado realmente se aplica al pasaje...); 2] startLoc (start location시작 위치inicio de la localizaciónEmplacement de début posizione iniziale); 3] endLoc (end location종료 위치final de la localizaciónemplacement de finposizione finale); 4] location; 5] attrName (attribute name속성명nom de
    ##             l'attribut.nome di attributoel valor dado para el nombre del atributo es incierto.); 6] transcribedContent; 7] suppliedContent
    attribute locus {
      
      ##  (element name) 不確定所使用的元素是否適用在該段落。
      "gi"
      | 
        ##  (start location) start-tag may not be correctly located.
        "startLoc"
      | 
        ##  (end location) end-tag may not be correctly located.
        "endLoc"
      | 
        ## 起始標籤與結束標籤也許都被錯置。
        "location"
      | 
        ##  (attribute name) 不確定該屬性名稱所使用的屬性值。
        "attrName"
      | 
        ## 轉錄在元素裡的內容也許和來源文本不相符合。
        "transcribedContent"
      | 
        ## 標記者所提供的元素內容也許有誤，例如在元素corr和元素abbrev出現的情況。
        "suppliedContent"
      | xsd:Name
    },
    
    ## 提供一個替代屬性值給指定標記部分—一個替代的類目識別符碼、轉錄、或屬性值，或是元素anchor (用來指出一個替代的開始或結束位置) 的識別符碼。若使用屬性assertedValue，則屬性degree中指出的確定程度適用於屬性assertedValue所指明的替代標記；若無使用，則確定程度適用於文本中的標記。
    attribute assertedValue {
      xsd:anyURI
      | xsd:Name
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    
    ## 指出在評估確定程度時所設想的情況。
    attribute given {
      list { xsd:anyURI+ }
    }?,
    
    ## 指出屬性locus所指定的標記部分之確定程度。
    attribute degree {
      xsd:double { minInclusive = "0" maxInclusive = "1" }
    }?,
    empty
  }
respons =
  
  ##  (責任) 指出標記一或多個特定元素某部分的負責人。
  element respons {
    model.glossLike*,
    att.global.attributes,
    
    ## 指出標記責任所在元素的識別符碼。
    attribute target {
      list { xsd:anyURI+ }
    },
    
    ## 指出標記責任所屬的特定部分。
    ## Suggested values include: 1] gi (element name요소명Responsabilité de l'assertion selon laquelle
    ##               l'information encodée est du type indiqué par le balisage.nome di elementoresponsabilidad por la necesidad que un elemento sea del tipo indicado por el marcaje); 2] location; 3] startLoc (start location시작 위치inicio de la localizaciónemplacement de débutposizione iniziale); 4] endLoc (end location종료 위치final de la localizaciónemplacement de finposizione finale); 5] attrName (attribute name속성명Responsabilité de l'assertion selon laquelle
    ##               le nom de l'attribut utilisé correspond bien à l'information que l'on veut
    ##             donnernome di attributoresponsabilidad por la necesidad que el nombre del atributo tenga el valor dado en el marcaje.); 6] transcribedContent; 7] suppliedContent
    attribute locus {
      list {
        (
         ##  (element name) 負責聲明元素屬於標記所指示的類型
         "gi"
         | 
           ## 負責聲明元素依照所指示的位置開始與結束
           "location"
         | 
           ##  (start location) responsibility for the claim that the element begins
           ##           where indicated
           "startLoc"
         | 
           ##  (end location) responsibility for the claim that the element ends
           ##           where indicated
           "endLoc"
         | 
           ##  (attribute name) 負責聲明名稱屬性擁有標記中提供的屬性值
           "attrName"
         | 
           ## 負責元素內容的轉錄
           "transcribedContent"
         | 
           ## 負責標記者所提供的內容 (更正、縮寫還原等。) 
           "suppliedContent"
         | xsd:Name)+
      }
    },
    
    ##  (responsible party) 指出負責電子文本指定部分的個人或機構。
    attribute resp { xsd:anyURI },
    empty
  }
g =
  
  ##  (文字或字體) 非標準的文字或字體。
  element g {
    text,
    att.global.attributes,
    att.typed.attributes,
    
    ## 連結到該文字或字體的描述。
    attribute ref { xsd:anyURI }?,
    empty
  }
char =
  
  ##  (文字) 提供一個文字的描述性資訊。
