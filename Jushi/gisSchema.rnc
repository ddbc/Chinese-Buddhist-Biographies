    attribute indexName { xsd:Name }?,
    empty
  }
milestone =
  
  ## 標明文本中由標準參照系統變更所指示的任何區塊分界點，並且該章節未以任何結構性元素標記。
  element milestone {
    empty,
    att.global.attributes,
    att.typed.attributes,
    att.sourced.attributes,
    
    ## 提供此分界段落所使用的通用單位名稱。
    ## Suggested values include: 1] page; 2] column; 3] line; 4] book; 5] poem; 6] canto; 7] speaker; 8] stanza; 9] act; 10] scene; 11] section; 12] absent; 13] unnumbered
    attribute unit {
      
      ## 分頁
      ##                                                   (和元素pb同義) 。
      "page"
      | 
        ## 分段。
        "column"
      | 
        ## 分行
        ##                                                   (和元素lb同義) 。
        "line"
      | 
        ## 所有稱為書、冊、或函等的單位。
        "book"
      | 
        ## 詩集中的單首詩。
        "poem"
      | 
        ## 長詩的詩篇，或詩的大段落。
        "canto"
      | 
        ## changes of speaker or narrator.
        "speaker"
      | 
        ## 一首詩、一本書、或一詩篇中的詩節。
        "stanza"
      | 
        ## 劇本中的幕。
        "act"
      | 
        ## 劇本中的景。
        "scene"
      | 
        ## 任何種類的段落。
        "section"
      | 
        ## 該段文字未在參照版本中出現。
        "absent"
      | 
        ## passages present in the text, but not to be
        ##                                                   included as part of the
        ##                                                   reference.
        "unnumbered"
      | xsd:Name
    },
    empty
