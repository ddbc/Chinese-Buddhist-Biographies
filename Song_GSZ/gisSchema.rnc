p =
  
  ##  (段落) 標記散文的段落。
  element ns1:p {
    macro.paraContent,
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
foreign =
  
  ##  (外文) 標明所屬語言不同於週遭文字的字詞。
  element ns1:foreign { macro.phraseSeq, att.global.attributes, empty }
said =
  
  ##  (speech or thought) indicates passages thought or spoken aloud, whether explicitly indicated in the source or
  ##     not, whether directly or indirectly reported, whether by real people or fictional characters.
  element ns1:said {
    macro.specialPara,
    att.global.attributes,
    att.ascribed.attributes,
    
    ## may be used to indicate whether the quoted matter is regarded as having been vocalized
    ##         or signed.
    [ a:defaultValue = "unknown" ]
    attribute aloud { data.xTruthValue }?,
    
    ## may be used to indicate whether the quoted matter is regarded as direct or indirect
    ##         speech.
    [ a:defaultValue = "true" ] attribute direct { data.xTruthValue }?,
    empty
  }
quote =
  
  ##  (引文) 包含敘述者或作者引用自文本以外來源的字 (句) 詞或段落。
  element ns1:quote {
    macro.specialPara,
    att.global.attributes,
    att.typed.attributes,
    empty
  }
q =
  
  ##  (引用的言詞、思想、或文字) 包含標記為 (看似)
  ##     引用自他處的內容；在敘述文中，此元素用來標記直接或間接敘述；在字典中，用來標記實例或編者設計的範例；在寫本描述或其他上層資訊中，用來標記由來源文本中所摘錄的引文。
  element ns1:q {
    macro.specialPara,
    att.global.attributes,
    att.ascribed.attributes,
    
    ## 可用來指出引文內容為言詞或思想，或者做更詳細的引文描述。
    ## 被推薦的值包含: 1] spoken; 2] thought; 3] written; 4] soCalled; 5] foreign (foreign words) ; 6] distinct (linguistically distinct) ; 7] term (technical term) ; 8] emph (rhetorically emphasized) ; 9] mentioned
    attribute type {
      
      ## 表示直接敘述，通常會標上引號。
      "spoken"
      | 
        ## 表示思考內容，例如內心獨白。
        "thought"
      | 
        ## 引用自一份書面來源文件。
        "written"
      | 
        ## authorial distance
        "soCalled"
      | 
        ##  (foreign words) 
        "foreign"
      | 
        ##  (linguistically distinct) 
        "distinct"
      | 
        ##  (technical term) 
        "term"
      | 
        ##  (rhetorically emphasized) 
        "emph"
      | 
        ## refering to itself, not its normal referant
        "mentioned"
      | xsd:Name
    }?,
    empty
  }
cit =
  
  ## 從其他文件中引用的內容，並包含引用來源的書目參照資料。
  element ns1:cit {
    (model.qLike
     | model.egLike
     | model.biblLike
     | model.ptrLike
     | model.global
     | model.entryPart)+,
    att.global.attributes,
    att.typed.attributes,
    empty
  }
desc =
  
  ##  (描述) 包含一段簡短描述，說明一元素、屬性、或屬性值的應用與目的。
  element ns1:desc {
    macro.limitedContent,
    att.global.attributes,
    att.translatable.attributes,
    empty
  }
gloss =
  
  ## 標明用來為另一個字詞下定義或提供註解的字詞。
  element ns1:gloss {
    macro.phraseSeq,
    att.global.attributes,
    att.declaring.attributes,
    att.translatable.attributes,
    att.typed.attributes,
    att.pointing.attributes,
    
    ##  (canonical reference) 用元素refsDecl在TEI標頭內所定義的標準參照來指明相關聯的元素term
    ##          
    attribute cRef { data.pointer }?,
    empty
  }
term =
  
  ## 包含作為專有名詞的一個單字、詞彙、或是表示符號。
  element ns1:term {
    macro.phraseSeq,
    att.global.attributes,
    att.declaring.attributes,
    att.pointing.attributes,
    att.typed.attributes,
    att.canonical.attributes,
    
    ## identifies the associated gloss element using a canonical reference from a
    ##           scheme defined in a refsDecl element in the TEI header
    attribute cRef { data.pointer }?,
    
    ## 提供索引中該專有名詞的排序關鍵。
    attribute sortKey { data.word }?,
    empty
  }
sic =
  
  ##  (原文照錄) 包含看似錯誤但仍照實轉錄的文字 。
  element ns1:sic {
    macro.paraContent,
    att.global.attributes,
    att.responsibility.attributes,
    empty
  }
corr =
  
  ##  (更正) 包含文本中看似錯誤並加以更正過後的文字。
  element ns1:corr {
    macro.paraContent,
    att.global.attributes,
    att.editLike.attributes,
    att.typed.attributes,
    empty
  }
choice =
  
  ## 匯集文件中對於同一部分文字所有可供替換的不同標記。
  element ns1:choice {
    (model.choicePart | choice)*,
    
    ## Resp statement for the choice.
    attribute resp {
      xsd:string { pattern = "(sgsz|makita|fan)" }
    }?,
    att.global.attributes,
    empty
  }
reg =
  
  ##  (一般化) 標誌一般化或規格化處理過的文字。
  element ns1:reg {
    macro.paraContent,
    att.global.attributes,
    att.editLike.attributes,
    att.typed.attributes,
    empty
  }
orig =
  
  ##  (原文格式) 標誌原文中呈現的文字，並且未經過任何修改或規格化處理。
  element ns1:orig {
    macro.paraContent,
    att.global.attributes,
    att.responsibility.attributes,
    empty
  }
gap =
  
  ##  (gap) 指出轉錄時被省略部分的位置，省略也許是出於
  ##     TEI標頭裡描述的編輯上的理由、也許是因為抽樣轉錄而省略、或是因為資料不明難以辨認或聽懂。
  element ns1:gap {
    model.glossLike*,
    att.global.attributes,
    att.editLike.attributes,
    
    ## 說明省略的原因。屬性值的例子有sampling、、inaudible、irrelevant、cancelled、cancelled
    ##           and illegible。
    attribute reason {
      list { data.word, data.word* }
    }?,
    
    ## 若省略的內容是被身分明確的抄寫員由轉錄當中刻意刪除，則在此表明抄寫員的身分。
    attribute hand { data.pointer }?,
    
    ## 若省略是由於內容遭受損毀，且可識別損毀原因，則針對損毀原因加以分類。
    ## 實例值包含: 1] rubbing; 2] mildew; 3] smoke
    attribute agent { data.enumerated }?,
    empty
  }
add =
  
  ##  (插入) 包含由作者、抄寫者、註解者、或更正者在文本中插入的字母、單字或詞彙。
  element ns1:add {
    macro.paraContent,
    att.global.attributes,
    att.transcriptional.attributes,
    att.placement.attributes,
    att.typed.attributes,
    empty
  }
del =
  
  ##  (刪除) 標記在文本中，由作者、抄寫者、註解者、或更正者刪除、標上刪除記號、或者標明為多餘或偽造的字母或單字。
  element ns1:del {
    macro.paraContent,
    att.global.attributes,
    att.transcriptional.attributes,
    att.typed.attributes,
    empty
  }
name =
  
  ##  (名稱，特定名稱) 包含一個特定名稱 或名詞詞組。
  element ns1:name {
    macro.phraseSeq,
    att.global.attributes,
    att.naming.attributes,
    att.typed.attributes,
    empty
  }
rs =
  
  ##  (參照字串) 包含一般名稱或參照字串。
  element ns1:rs {
    macro.phraseSeq,
    att.global.attributes,
    att.naming.attributes,
    
    ## 更詳細指出參照字串所表示的對象。屬性值可包含人物、地點、船、元素等。
    attribute type { data.enumerated }?,
    empty
  }
email =
  
  ## 包含一個電子郵件位址，標明電子郵件訊息可傳送的位置。
  element ns1:email { macro.phraseSeq, att.global.attributes, empty }
date =
  
  ## 包含任何格式的日期表示。
  element ns1:date {
    (text | model.gLike | model.phrase | model.global)*,
    
    ## The unique identifier for this date as derived from the DDBC Date
    ##                                 Authority Database.
    attribute key {
      xsd:string { pattern = "(j\d{14})|unknown" }
    }?,
    
    ## 指明該日期表示所使用的曆法計算系統。
    ## 被推薦的值包含: 1] Gregorian; 2] Julian; 3] Islamic; 4] Hebrew; 5] Revolutionary; 6] Iranian; 7] Coptic; 8] Chinese
    attribute calendar {
      
      ## 格里高里曆
      "Gregorian"
      | 
        ## 凱撒曆
        "Julian"
      | 
        ## 伊斯蘭或穆斯林 (哈吉來) 陽曆
        "Islamic"
      | 
        ## 希伯來或猶太陰陽曆
        "Hebrew"
      | 
        ## 法國共和曆 
        "Revolutionary"
      | 
        ## 伊朗或波斯 (Jalaali) 陽曆
        "Iranian"
      | 
        ## 埃及古曆或亞歷山大曆
        "Coptic"
      | 
        ## 中國陰陽曆
        "Chinese"
      | xsd:Name
    }?,
    att.datable.iso.attributes,
    att.duration.iso.attributes,
    att.editLike.attributes,
    att.typed.attributes,
    empty
  }
time =
  
  ## 包含一組字詞，以任何形式定義時間
  element ns1:time {
    (text | model.gLike | model.phrase | model.global)*,
    att.global.attributes,
    att.datable.attributes,
    att.editLike.attributes,
    att.typed.attributes,
    empty
  }
abbr =
  
  ##  (縮寫) 包含任何形式的縮寫。
  element ns1:abbr {
    macro.phraseSeq,
    att.global.attributes,
    att.responsibility.attributes,
    
    ## 標記者可以用合宜的分類方法將縮寫形式分類。
    ## 實例值包含: 1] suspension; 2] contraction; 3] brevigraph; 4] superscription; 5] acronym; 6] title; 7] organization; 8] geographic
    attribute type { data.enumerated }?,
    empty
  }
expan =
  
  ##  (縮寫還原) 包含一個縮寫詞的還原形式。
  element ns1:expan {
    macro.phraseSeq,
    att.global.attributes,
    att.editLike.attributes,
    empty
  }
ptr =
  
  ## 標明一個與其他位置相連結的指標。
  element ns1:ptr {
    empty
    >> sch:pattern [
         id = "ptr-constraint-ptrAtts"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ptr"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "	attributes 'target' and 'cRef' may be supplied."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    att.pointing.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    
    ##  (canonical reference) 用元素refsDecl在TEI標頭內所定義的標準參照來說明指標所指位置。
    attribute cRef {
      list { data.word, data.word* }
    }?,
    empty
  }
ref =
  
  ## 標明與其他位置互相參照的符號，或許包含附加的文字或註解。
  element ns1:ref {
    macro.paraContent
    >> sch:pattern [
         id = "ref-constraint-refAtts"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ref"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "	attributes 'target' and 'cRef' may be supplied."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    
    ##
    attribute key {
      xsd:string { pattern = "(A\d{6})|(G\d{6})" }
    }?,
    
    ##  (canonical reference) 用元素refsDecl在TEI標頭內所定義的標準參照來說明參照所指位置。
    attribute cRef {
      list { data.word, data.word* }
    }?,
    att.global.attributes,
    att.pointing.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    empty
  }
\list =
  
  ##  (list) 包含以列表方式呈現的任何連續項目。
  element ns1:list {
    ((model.divTop | model.global)*,
     ((item, model.global*)+
      | (label, model.global*, item, model.global*)+),
     (model.divBottom, model.global*)*),
    att.global.attributes,
    
    ## 描述列表的形式。
    ## 被推薦的值包含: 1] ordered; 2] bulleted; 3] simple; 4] gloss
    [ a:defaultValue = "simple" ]
    attribute type {
      
      ## 列表項目以數字或字母依順序排列。
      "ordered"
      | 
        ## 列表項目以黑點或其他印刷符號排列。
        "bulleted"
      | 
        ## 列表項目的排列沒有用數字或任何其他符號作標記。
        "simple"
      | 
        ## 各列表項目為一些字詞或概念下註解，每個項目之前並帶有標籤元素。
        "gloss"
      | xsd:Name
    }?,
    empty
  }
item =
  
  ## 包含列表中的一個項目。
  element ns1:item { macro.specialPara, att.global.attributes, empty }
label =
  
  ## 包含和列表中某項目相關聯的標號；在詞彙表中則用來標記被定義的字詞。
  element ns1:label { macro.phraseSeq, att.global.attributes, empty }
head =
  
  ##  (標題) 包含任何種類的標題，如章節、列表、詞彙表、寫本描述等的標題。
  element ns1:head {
    macro.paraContent,
    att.global.attributes,
    att.typed.attributes,
    empty
  }
note =
  
  ## 包含附註或註釋。
  element ns1:note {
    macro.specialPara,
    att.global.attributes,
    att.placement.attributes,
    att.pointing.attributes,
    att.responsibility.attributes,
    att.typed.attributes,
    
    ## 說明來源文件是否指出附註的確切參照位置。
    [ a:defaultValue = "true" ] attribute anchored { data.truthValue }?,
    
    ## 如果附註不是嵌在正文中，則用以標明附註所註段落的終點。
    attribute targetEnd {
      list { data.pointer, data.pointer* }
    }?,
    empty
  }
index =
  
  ##  (索引項目) 標誌一個因某種目的而被索引的位置。
  element ns1:index {
    (term, index?)*,
    att.global.attributes,
    att.spanning.attributes,
    
    ## 提供該索引項目所屬索引名稱。
    attribute indexName { data.name }?,
    empty
  }
milestone =
  
  ## 標明文本中由標準參照系統變更所指示的任何區塊分界點，並且該章節未以任何結構性元素標記。
  element ns1:milestone {
    empty,
    att.global.attributes,
    att.typed.attributes,
    att.sourced.attributes,
    att.spanning.attributes,
    att.breaking.attributes,
    
    ## 提供此分界段落所使用的通用單位名稱。
    ## 被推薦的值包含: 1] page; 2] column; 3] line; 4] book; 5] poem; 6] canto; 7] speaker; 8] stanza; 9] act; 10] scene; 11] section; 12] absent; 13] unnumbered
    attribute unit {
      
      ## 分頁 (和元素pb同義) 。
      "page"
      | 
        ## 分段。
        "column"
      | 
        ## 分行 (和元素lb同義) 。
        "line"
      | 
        ## 所有稱為書、冊、或函等的單位。
        "book"
      | 
        ## 詩集中的單首詩。
        "poem"
      | 
        ## 長詩的詩篇，或詩的大段落。
        "canto"
      | 
        ## changes of speaker or narrator.
        "speaker"
      | 
        ## 一首詩、一本書、或一詩篇中的詩節。
        "stanza"
      | 
        ## 劇本中的幕。
        "act"
      | 
        ## 劇本中的景。
        "scene"
      | 
        ## 任何種類的段落。
        "section"
      | 
        ## 該段文字未在參照版本中出現。
        "absent"
      | 
        ## passages present in the text, but not to be included as part of the reference.
        "unnumbered"
      | xsd:Name
    },
    empty
  }
pb =
  
  ##  (分頁) 以標準參照系統來標記頁與頁之間的分界線。
  element ns1:pb {
    empty,
    att.global.attributes,
    att.typed.attributes,
    att.sourced.attributes,
    att.spanning.attributes,
    att.breaking.attributes,
    empty
  }
lb =
  
  ##  (分行) 標記某版本文本裡 (在印刷上) 的新起行。
  element ns1:lb {
    empty,
    att.global.attributes,
    att.typed.attributes,
    att.sourced.attributes,
    att.spanning.attributes,
    att.breaking.attributes,
    empty
  }
author =
  
  ## 在書目參照中，包含一件作品的作者 (群) 姓名，無論是個人或是團體性質；這也是書目項目責任歸屬的主要陳述。
  element ns1:author {
    macro.phraseSeq, att.global.attributes, att.naming.attributes, empty
  }
editor =
  
  ## 書目項目的次要責任歸屬，例如扮演編輯、編譯、翻譯等角色的個人、機構或組織 (或其他類似者) 的名稱。
  element ns1:editor {
    macro.phraseSeq, att.global.attributes, att.naming.attributes, empty
  }
respStmt =
  
  ##  (責任陳述) 當未使用作者、編輯者等特定元素，或元素內容不足時，在此補充說明文件、版本、記錄、或是叢書的智慧內容所屬負責人的責任陳述。
  element ns1:respStmt {
    ((resp+, model.nameLike.agent+) | (model.nameLike.agent+, resp+)),
    att.global.attributes,
    empty
  }
resp =
  
  ## 包含一個詞彙，來描述個人智慧責任的類型。
  element ns1:resp {
    macro.phraseSeq.limited,
    att.global.attributes,
    att.canonical.attributes,
    empty
  }
title =
  
  ## 包含任何種類作品的完整題名
  ##                     。
  element ns1:title {
    macro.paraContent,
    att.global.attributes,
    att.canonical.attributes,
    
    ## 指出題名的書目層次，即該題名之所指可以是文章、書籍、期刊、叢書或未出版的項目。
    attribute level {
      
      ##  (analytic) 分析層題名
      ##                                                   (文章、詩、或其他隸屬於一個較大作品的項目) 
      "a"
      | 
        ##  (monographic) 專題層題名
        ##                                                   (書、選集、或其他獨立出版的項目，包含多冊作品的其中一冊) 
        "m"
      | 
        ##  (journal) 期刊層題名
        "j"
      | 
        ##  (series) 叢書層題名
        "s"
      | 
        ##  (unpublished) 未出版的項目題名 (包括未經商業刊物發表的專題論文) 
        "u"
    }?,
    
    ## 用合適的分類方法將題名分類。
    ## 實例值包含: 1] main; 2] sub (subordinate) ; 3] alt (alternate) ; 4] short; 5] desc (descriptive) 
    attribute type { data.enumerated }?,
    empty
  }
publisher =
  
  ## 提供負責出版或發行書目項目的機構名稱。
  element ns1:publisher {
    macro.phraseSeq, att.global.attributes, empty
  }
pubPlace =
  
  ## 包含書目項目的出版地名稱。
  element ns1:pubPlace {
    macro.phraseSeq, att.global.attributes, att.naming.attributes, empty
  }
bibl =
  
  ##  (書目資料) 包含結構零散的書目資料，其中次要元件不一定會明確標記。
  element ns1:bibl {
    (text
     | model.gLike
     | model.highlighted
     | model.pPart.data
     | model.pPart.edit
     | model.segLike
     | model.ptrLike
     | model.biblPart
     | model.global)*,
    att.global.attributes,
    att.declarable.attributes,
    att.typed.attributes,
    empty
  }
biblStruct =
  
  ##  (結構次要書目) 包含僅出現子節點的結構書目資料，並以特定順序呈現。
  element ns1:biblStruct {
    (model.noteLike | idno | relatedItem)*,
    att.global.attributes,
    att.declarable.attributes,
    att.typed.attributes,
    empty
  }
listBibl =
  
  ##  (書目列表) 包含任何種類的書目資料列表。
  element ns1:listBibl {
    (model.headLike*,
     (model.biblLike | model.milestoneLike | listBibl)+),
    att.global.attributes,
    att.declarable.attributes,
    att.typed.attributes,
    empty
  }
relatedItem =
  
  ## 包含或參照到其他書目項目，該些項目與目前的書目項目在某種程度上相關，例如作為其組成或者替代項目。
  element ns1:relatedItem {
    ((model.biblLike | model.ptrLike)?)
    >> sch:pattern [
         id = "relatedItem-constraint-targetorcontent11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relatedItem"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and count( child::* ) > 0"
             "If the 'target' attribute is used, the\x{a}" ~
             "        relatedItem element must be empty"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "relatedItem-constraint-targetorcontent11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relatedItem"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@target or child::*"
             "A relatedItem element should have either a 'target' attribute\x{a}" ~
             "        or a child element to indicate the related bibliographic item"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    att.typed.attributes,
    
    ## points to the related bibliographic element by means of an
    ##       absolute or relative URI reference
    attribute target { data.pointer }?,
    empty
  }
l =
  
  ##  (詩行) 包含詩文的一行，也許是不完整的詩行。
  element ns1:l {
    macro.paraContent,
    att.global.attributes,
    
    ## 說明詩行的韻律表現是否完整。
    [ a:defaultValue = "N" ]
    attribute part {
      
      ##  (yes) 韻律表現不完整的詩行
      "Y"
      | 
        ##  (no) 詩行完整或未說明其完整性
        "N"
      | 
        ##  (initial) 不完整詩行的起始部分
        "I"
      | 
        ##  (medial) 不完整詩行的中段部分
        "M"
      | 
        ##  (final) 不完整詩行的末端部分
        "F"
    }?,
    empty
  }
lg =
  
  ##  (行組) 包含形式上視為一組的詩行，例如詩節、疊句、韻文段落等。 
  element ns1:lg {
    ((model.divTop | model.global)*,
     (model.lLike | lg),
     (model.lLike | lg | model.global)*,
     (model.divBottom, model.global*)*),
    att.global.attributes,
    att.divLike.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    empty
  }
teiCorpus =
  
  ## 包含一套用TEI編碼的文件集，由單一文集標頭以及一個 (或多個)
  ##     TEI元素所組成，各TEI元素包含單一文本標頭和一個文本。
  element ns1:teiCorpus {
    (teiHeader, (TEI | teiCorpus)+),
    att.global.attributes,
    
    ## TEI架構的版本
    [ a:defaultValue = "5.0" ] attribute version { data.version }?,
    empty
  }
teiHeader =
  
  ##  (TEI標頭) 在所有符合TEI標準的文本起始的電子題名頁當中提供敘述性以及宣告性的資訊。
  element ns1:teiHeader {
    (fileDesc, model.teiHeaderPart*, revisionDesc?),
    att.global.attributes,
    
    ## 標明標頭所附屬於的文件類型，例如文集或單一文本。
    ## 實例值包含: 1] text; 2] corpus
    [ a:defaultValue = "text" ] attribute type { data.enumerated }?,
    empty
  }
fileDesc =
  
  ##  (檔案描述) 包含對電子檔案的完整書目敘述。
  element ns1:fileDesc {
    ((titleStmt,
      editionStmt?,
      extent?,
      publicationStmt,
      seriesStmt?,
      notesStmt?),
     sourceDesc+),
    att.global.attributes,
    empty
  }
titleStmt =
  
  ##  (題名陳述) 匯集關於作品標題與作品智慧內容責任歸屬的資訊。
  element ns1:titleStmt {
    (title+, model.respLike*), att.global.attributes, empty
  }
sponsor =
  
  ## 標明贊助的組織或機構名稱。
  element ns1:sponsor {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
funder =
  
  ##  (資助者) 標明負責資助一項計畫或文件製作的個人、機構或組織名稱。
  element ns1:funder {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
principal =
  
  ##  (主導研究者) 提供負責電子文件製作的主導研究者的姓名。
  element ns1:principal {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
editionStmt =
  
  ##  (版本陳述) 匯集文件某一版本的相關資訊。
  element ns1:editionStmt {
    (model.pLike+ | (edition, respStmt*)),
    att.global.attributes,
    empty
  }
edition =
  
  ##  (版本) 描述文件某一版本的特質。
  element ns1:edition { macro.phraseSeq, att.global.attributes, empty }
extent =
  
  ## 描述電子文件儲存在某一承載媒介時的約略大小，標以任何適用的單位。
  element ns1:extent { macro.phraseSeq, att.global.attributes, empty }
publicationStmt =
  
  ##  (出版陳述) 匯集電子文件或其他類型文件的出版或發行相關資訊。
  element ns1:publicationStmt {
    (model.pLike+ | model.publicationStmtPart+),
    att.global.attributes,
    empty
  }
distributor =
  
  ## 提供負責發行文件的個人或其他經銷商的名稱。
  element ns1:distributor {
    macro.phraseSeq, att.global.attributes, empty
  }
authority =
  
  ##  (供應機構) 說明除了出版者或發行者之外，負責提供電子檔案的個人或其他經銷商的名稱。
  element ns1:authority {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
idno =
  
  ##  (識別代碼) 提供任何用來識別書目項目的標準或非標準編碼。
  element ns1:idno {
    macro.xtext,
    att.global.attributes,
    
    ## 表明編碼的種類，例如國際標準書號 (ISBN) 或其他標準系列編碼。
    attribute type { data.enumerated }?,
    empty
  }
availability =
  
  ## 提供關於文件可利用性的資訊，例如使用或發行上的任何限制、版權狀態等。
  element ns1:availability {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ## 提供一個代號，用來確認文件目前的可利用性。
    [ a:defaultValue = "unknown" ]
    attribute status {
      
      ## 文件的可利用性不受限。
      "free"
      | 
        ## 文件狀態不明。
        "unknown"
      | 
        ## 文件的可利用性受限制。
        "restricted"
    }?,
    empty
  }
seriesStmt =
  
  ##  (集叢陳述) 若出版品隸屬於某叢書，則在此匯集該叢書的相關資訊。
  element ns1:seriesStmt {
    (model.pLike+
     | (title+, (idno | respStmt)*)),
    att.global.attributes,
    empty
  }
notesStmt =
  
  ##  (附註陳述) 收集其他部分的書目描述中所未記載的任何文件相關附註。
  element ns1:notesStmt {
    model.noteLike+, att.global.attributes, empty
  }
sourceDesc =
  
  ## 提供所製電子文件的一個或數個來源文件的書目描述。
  element ns1:sourceDesc {
    (model.pLike+
     | (model.biblLike | model.sourceDescPart | model.listLike)+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
biblFull =
  
  ## 包含結構完整的書目資料，其中呈現出TEI檔案描述內的所有元件。
  element ns1:biblFull {
    ((titleStmt,
      editionStmt?,
      extent?,
      publicationStmt,
      seriesStmt?,
      notesStmt?),
     sourceDesc*),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
encodingDesc =
  
  ##  (編碼描述) 記錄電子文件和其ㄧ個或多個文件來源之間的關係。
  element ns1:encodingDesc {
    (model.encodingDescPart | model.pLike)+,
    att.global.attributes,
    empty
  }
projectDesc =
  
  ##  (計畫描述) 詳述將電子檔案編碼的目標或目的，以及其他關於檔案匯集或收集程序的任何資訊。
  element ns1:projectDesc {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
samplingDecl =
  
  ##  (取樣宣告) 以篇章描述的方式說明建立文集或文選時文件取樣的原理與方法。
  element ns1:samplingDecl {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
editorialDecl =
  
  ##  (編輯實行宣告) 提供文件編碼時使用的編輯原則與實行方法的細節。
  element ns1:editorialDecl {
    (model.pLike | model.editorialDeclPart)+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
correction =
  
  ##  (更正原則) 說明文本中的更正是在什麼情況下、如何產生的。
  element ns1:correction {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ## 指出文本更正的程度。
    [ a:defaultValue = "unknown" ]
    attribute status {
      
      ## 該文本以被徹底檢查校對過。
      "high"
      | 
        ## 該文本被檢查過至少一次。
        "medium"
      | 
        ## 該文本從未被檢查過。
        "low"
      | 
        ## 更正狀態不明
        "unknown"
    }?,
    
    ## 說明標示更正所採取的方法。
    [ a:defaultValue = "silent" ]
    attribute method {
      
      ## 直接在文本中更正
      "silent"
      | 
        ## corrections have been represented using markup
        "markup"
    }?,
    empty
  }
normalization =
  
  ## 指出來源文件轉換成電子文本時規格化或標準化的程度。
  element ns1:normalization {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ## 指出任何規格化所實行的依據。
    attribute source { data.pointer }?,
    
    ## 指出標示規格化所採取的方法。
    [ a:defaultValue = "silent" ]
    attribute method {
      
      ## 直接在文本中實行規格化
      "silent"
      | 
        ## normalization represented using markup 
        "markup"
    }?,
    empty
  }
quotation =
  
  ## 說明在編輯上是如何處理來源文件中的引文符號。
  element ns1:quotation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ##  (quotation marks) 指出引文符號是否在文本中被保留為內容的一部份。
    [ a:defaultValue = "all" ]
    attribute marks {
      
      ## 不保留任何引文符號
      "none"
      | 
        ## 保留部分引文符號
        "some"
      | 
        ## 保留所有引文符號
        "all"
    }?,
    
    ## 說明引文符號如何標示於文本中。
    [ a:defaultValue = "unknown" ] attribute form { text }?,
    empty
  }
hyphenation =
  
  ## 概述來源文件裡的連字符號在電子化的版本中是如何被處理的。
  element ns1:hyphenation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    
    ##  (end-of-line) 指出行末連字符號是否在文本中被保留。
    [ a:defaultValue = "some" ]
    attribute eol {
      
      ## 保留所有行末連字符號，即使原文中的不被保留。
      "all"
      | 
        ## 行末連字符號在某些情況下被保留。
        "some"
      | 
        ## 所有軟音的行末連字符號皆被移除：任何剩下的行末連字符號應該被保留。
        "hard"
      | 
        ## 所有行末連字符號皆被移除：剩下的是行中出現的行末連字符號。
        "none"
    }?,
    empty
  }
segmentation =
  
  ## 描述文件分割的原理方法，例如分割成句子、音調單位、字形層次等。
  element ns1:segmentation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
stdVals =
  
  ##  (標準值) 明確說明文本中標準化日期或數值所使用的格式。
  element ns1:stdVals {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
interpretation =
  
  ## 指出除了轉錄之外，文件中加入哪些分析性或詮釋性的資訊。
  element ns1:interpretation {
    model.pLike+,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
tagsDecl =
  
  ##  (標誌宣告) 提供關於應用在XML文件中的標誌的詳細資訊。
  element ns1:tagsDecl {
    (rendition*, \namespace*), att.global.attributes, empty
  }
tagUsage =
  
  ## 提供文件中某個特定元素的使用資訊。
  element ns1:tagUsage {
    macro.limitedContent,
    att.global.attributes,
    
    ##  (element name) 該標籤所指明的元素名稱 (通用識別符碼)。
    attribute gi { data.name },
    
    ## 指出該元素在文本中出現的次數。
    attribute occurs { data.count }?,
    
    ##  (with unique identifier) 指出帶有明確全域屬性xml:id屬性值的元素在文本中的出現次數。
    attribute withId { data.count }?,
    
    ## 標明元素rendition的識別符號，該符號定義此元素的呈現方法。
    attribute render { data.pointer }?,
    empty
  }
\namespace =
  
  ## 提供其子元素所描述的元素所屬的名稱空間之正式名稱。 
  element ns1:namespace {
    tagUsage+,
    att.global.attributes,
    
    ## 名稱空間的正式完整名稱。
    attribute name { data.namespace },
    empty
  }
rendition =
  
  ## 提供關於一個或多個元素預定呈現方式的資訊。
  element ns1:rendition {
    macro.limitedContent,
    att.global.attributes,
    
    ## identifies the language used to describe the rendition.
    attribute scheme {
      
      ## Cascading Stylesheet Language
      "css"
      | 
        ## Extensible Stylesheet Language Formatting Objects
        "xslfo"
      | 
        ## Informal free text description
        "free"
      | 
        ## A user-defined rendition description language
        "other"
    }?,
    
    ## where CSS is used, provides a way of defining
    ## pseudo-elements, that is, styling rules
    ## applicable to specific sub-portions of an element.
    attribute scope { text }?,
    empty
  }
refsDecl =
  
  ##  (參照宣告) 標明本文件與某一個權威參照標準的關係。
  element ns1:refsDecl {
    (model.pLike+ | cRefPattern+ | refState+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
cRefPattern =
  
  ##  (定義如何將標準參照轉換成統一資源識別符 (URI) 。) 標明將標準參照轉換成統一資源識別符的表示方法與取代模式。 
  element ns1:cRefPattern {
    model.pLike*,
    att.global.attributes,
    
    ## 標明一個表示屬性cRef屬性值的固定方法。
    attribute matchPattern { data.pattern },
    
    ## 標明一個取代模式，一旦進行次要模式的替換，會自動提供統一資源識別符 (URI) 。
    attribute replacementPattern { text },
    empty
  }
refState =
  
  ##  (分界狀態) 標明分界方法所定義的權威參照標準裡其中一個組件。
  element ns1:refState {
    empty,
    att.global.attributes,
    att.sourced.attributes,
    
    ## indicates what kind of state is changing at this milestone.
    ## 被推薦的值包含: 1] page; 2] column; 3] line; 4] book; 5] poem; 6] canto; 7] stanza; 8] act; 9] scene; 10] section; 11] absent
    attribute unit {
      
      ## page breaks in the reference edition. 
      "page"
      | 
        ## column breaks. 
        "column"
      | 
        ## line breaks. 
        "line"
      | 
        ## any units termed book, liber, etc. 
        "book"
      | 
        ## individual poems in a collection. 
        "poem"
      | 
        ## cantos or other major sections of a poem. 
        "canto"
      | 
        ## stanzas within a poem, book, or canto. 
        "stanza"
      | 
        ## acts within a play. 
        "act"
      | 
        ## scenes within a play or act. 
        "scene"
      | 
        ## sections of any kind. 
        "section"
      | 
        ## passages not present in the reference edition.
        "absent"
      | xsd:Name
    },
    
    ## specifies the fixed length of the reference component.
    attribute length { data.count }?,
    
    ##  (delimiter) supplies a delimiting string following the reference component.
    attribute delim { text }?,
    empty
  }
classDecl =
  
  ##  (分類宣告) 包含一個或多個分類法，用來定義在文件中他處所使用之任何分類碼。
  element ns1:classDecl { taxonomy+, att.global.attributes, empty }
taxonomy =
  
  ## 定義文件分類的類型學，可以是潛在地以書目資料的方式，或是明確地以結構分類法的方式來分類。
  element ns1:taxonomy {
    (model.glossLike* | category+ | (model.biblLike, category*)),
    att.global.attributes,
    empty
  }
category =
  
  ## 在使用者定義之分類法當中，此元素包含一項個別的描述性類目，該類目可能位於一個更上層的類目之中。
  element ns1:category {
    ((catDesc+ | model.glossLike*), category*),
    att.global.attributes,
    empty
  }
catDesc =
  
  ##  (類目描述) 描述分類法中或文件類型學中的某些類目，可用短文描述的型式，或是用TEI正式元素textDesc所使用的狀況參數。
  element ns1:catDesc {
    (text | model.limitedPhrase | model.catDescPart)*,
    att.global.attributes,
    empty
  }
appInfo =
  
  ##  (application information) records information about an application which has
  ##   edited the TEI file. 
  element ns1:appInfo {
    model.applicationLike+, att.global.attributes, empty
  }
application =
  
  ## provides information about an application which has acted upon the document.
  element ns1:application {
    (model.labelLike+, (model.ptrLike* | model.pLike*)),
    att.global.attributes,
    att.typed.attributes,
    att.datable.attributes,
    
    ## Supplies an identifier for the application, independent of its version number or display
    ##         name.
    attribute ident { data.name },
    
    ## Supplies a version number for the application, independent of its identifier or display
    ##         name.
    attribute version {
      xsd:token {
        pattern = "[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}"
      }
    },
    empty
  }
profileDesc =
  
  ##  (文件背景描述) 提供文件非書目性方面的細節描述，特別是所使用的語言及次要語言、在何種情況下製作、參與人員及其環境背景。
  element ns1:profileDesc {
    model.profileDescPart*, att.global.attributes, empty
  }
creation =
  
  ## 包含關於文件建置的資訊。 
  element ns1:creation {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
langUsage =
  
  ##  (語言使用) 描述文件中使用的語言、次要語言、語域、方言等。
  element ns1:langUsage {
    language+, att.global.attributes, att.declarable.attributes, empty
  }
language =
  
  ## 個別敘述文本中使用的語言或次要語言。
  element ns1:language {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ##  (identifier) 提供一語言代碼，其編碼方式定義在RFC 3066中 (或其系列中)
    ##           ，用來識別此元素所紀錄的語言，並由全域屬性xml:lang所參照。
    attribute ident { data.language },
    
    ## 標明該文本使用此語言的大約比例 (以冊計) 。
    attribute usage {
      xsd:nonNegativeInteger { maxInclusive = "100" }
    }?,
    empty
  }
textClass =
  
  ##  (文件分類) 匯集以標準分類架構、索引典等來描述文件性質或主題的資訊。
  element ns1:textClass {
    (classCode | catRef | keywords)*,
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
keywords =
  
  ## 包含標明文件主題或性質的關鍵詞或字詞列表。
  element ns1:keywords {
    (term+ | \list),
    att.global.attributes,
    
    ## 標明相關關鍵詞所使用到的詞彙範圍。
    attribute scheme { data.pointer },
    empty
  }
classCode =
  
  ##  (classification code) 包含文件在某標準分類系統中所屬的分類代碼。
  element ns1:classCode {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明使用中的分類系統或分類法。
    attribute scheme { data.pointer },
    empty
  }
catRef =
  
  ##  (類目參照) 標明在某分類法或文件類型學中，一個或多個已定義之類目。
  element ns1:catRef {
    empty,
    att.global.attributes,
    att.pointing.attributes,
    
    ## 標明該分類架構，其中相關的類目群組已被定義
    attribute scheme { data.pointer }?,
    empty
  }
revisionDesc =
  
  ##  (修訂描述) 概述檔案的修訂歷史。
  element ns1:revisionDesc {
    (\list | change+),
    att.global.attributes,
    att.docStatus.attributes,
    empty
  }
change =
  
  ## 概述多位研究者共享的某版本電子文本當中，某特定的內容變更或修正。
  element ns1:change {
    (text | model.limitedPhrase | model.inter | model.global)*,
    att.global.attributes,
    att.ascribed.attributes,
    att.datable.attributes,
    att.docStatus.attributes,
    empty
  }
geoDecl =
  
  ##  (geographic coordinates declaration) documents the notation and the datum used for geographic coordinates expressed as content of
  ##     the geo element elsewhere within the document.
  element ns1:geoDecl {
    macro.phraseSeq,
    att.global.attributes,
    att.declarable.attributes,
    
    ## supplies a commonly used code name for the datum employed.
    ## 被推薦的值包含: 1] WGS84 (World Geodetic System) ; 2] MGRS (Military Grid Reference System) ; 3] OSGB36 (ordnance survey great britain) ; 4] ED50 (European Datum coordinate system) 
    [ a:defaultValue = "WGS84" ]
    attribute datum {
      
      ##  (World Geodetic System) a pair of numbers to be interpreted as latitude followed by longitude according to
      ##             the World Geodetic System.
      "WGS84"
      | 
        ##  (Military Grid Reference System) the values supplied are geospatial entity object codes, based on
        "MGRS"
      | 
        ##  (ordnance survey great britain) the value supplied is to be interpreted as a British National Grid Reference.
        "OSGB36"
      | 
        ##  (European Datum coordinate system) the value supplied is to be interpreted as latitude followed by longitude according
        ##             to the European Datum coordinate system.
        "ED50"
      | xsd:Name
    }?,
    empty
  }
TEI =
  
  ##  (TEI文件) 包含符合TEI標準的單一文件，由一個TEI標頭及一份文本組成，可單獨出現或是作為元素teiCorpus的一部分。
  element ns1:TEI {
    (teiHeader,
     ((model.resourceLike+, \text?) | \text))
    >> sch:ns [ prefix = "tei" uri = "http://www.tei-c.org/ns/1.0" ]
    >> sch:ns [
         prefix = "rng"
         uri = "http://relaxng.org/ns/structure/1.0"
       ],
    att.global.attributes,
    
    ## TEI架構的版本
    attribute version { data.version }?,
    empty
  }
\text =
  
  ## 包含一份任何種類的文本，無論是單一或複合的，例如詩詞或戲劇、散文集、小說、字典、或是文集範例。
  element ns1:text {
    (model.global*,
     (front, model.global*)?,
     (body | group),
     model.global*,
     (back, model.global*)?),
    att.global.attributes,
    att.declaring.attributes,
    att.typed.attributes,
    empty
  }
body =
  
  ##  (正文) 單篇文章的整體部分，不包含正文前及正文後資訊。
  element ns1:body {
    (model.global*,
     (model.divTop, (model.global | model.divTop)*)?,
     (model.divGenLike, (model.global | model.divGenLike)*)?,
     ((model.divLike, (model.global | model.divGenLike)*)+
      | (model.div1Like, (model.global | model.divGenLike)*)+
      | ((model.common, model.global*)+,
         ((model.divLike, (model.global | model.divGenLike)*)+
          | (model.div1Like, (model.global | model.divGenLike)*)+)?)),
     (model.divBottom, model.global*)*),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
group =
  
  ## 包含複合文本的正文，所匯集的一連串單一文件 (或文件群組)
  ##     ，因為某特殊目的被視為一個整體，例如一個作者的作品集合、一連串的散文集等。
  element ns1:group {
    ((model.divTop | model.global)*,
     ((\text | group), (\text | group | model.global)*),
     model.divBottom*),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
\div =
  
  ##  (文本區段) 文本的正文前資訊、正文、及正文後資訊的分段。
  element ns1:div {
    ((model.divTop | model.global)*,
     ((((model.divLike | model.divGenLike), model.global*)+
       | ((model.common, model.global*)+,
          ((model.divLike | model.divGenLike), model.global*)*)),
      (model.divBottom, model.global*)*)?),
    att.global.attributes,
    att.divLike.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    empty
  }
byline =
  
  ## 包含作品的主要責任陳述，出現在題名頁或作品開頭或結尾處。
  element ns1:byline {
    (text | model.gLike | model.phrase | model.global)*,
    att.global.attributes,
    empty
  }
postscript =
  
  ## contains a postscript, e.g. to a letter.
  element ns1:postscript {
    (model.common | model.global)*, att.global.attributes, empty
  }
front =
  
  ##  (正文前資訊) 包含位於文件最前端、正文之前的項目 (標頭、題名頁、前言、獻詞等) 。 
  element ns1:front {
    ((model.frontPart | model.pLike.front | model.global)*,
     ((model.div1Like,
       (model.frontPart | model.div1Like | model.global)*)
      | (model.divLike,
         (model.frontPart | model.divLike | model.global)*))?,
     (model.divBottomPart, (model.divBottomPart | model.global)*)?),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
back =
  
  ##  (正文後資訊) 在正文之後，包含附錄等。
  element ns1:back {
    ((model.frontPart | model.pLike.front | model.global)*,
     ((model.div1Like,
       (model.frontPart | model.div1Like | model.global)*)
      | (model.divLike,
         (model.frontPart | model.divLike | model.global)*))?,
     (model.divBottomPart, (model.divBottomPart | model.global)*)?),
    att.global.attributes,
    att.declaring.attributes,
    empty
  }
s =
  
  ##  (句子單元) 包含文字中一個句子組成的區段。
  element ns1:s {
    macro.phraseSeq
    >> sch:pattern [
         id = "s-constraint-noNestedS"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:s"
           "\x{a}" ~
           "               "
           sch:report [
             test = "tei:s"
             "You may not nest one s element within\x{a}" ~
             "      another: use seg instead"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    empty
  }
w =
  
  ##  (單字) 表示文法上 (但未必是拼字法上) 定義的單字。
  element ns1:w {
    (text
     | model.gLike
     | seg
     | w
     | c
     | model.global
     | model.lPart
     | model.hiLike
     | model.pPart.edit)*,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    
    ## 指出在字典中該字的詞條形式。
    attribute lemma { data.key }?,
    
    ## provides a pointer to a definition of the lemma for the
    ##       word, for example in an online lexicon. 
    attribute lemmaRef { data.pointer }?,
    empty
  }
c =
  
  ##  (文字符號) 表示一個文字符號。
  element ns1:c {
    macro.xtext,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    empty
  }
pc =
  
  ##  (punctuation character) a character or string of characters regarded as constituting a
  ##   single punctuation mark.
  element ns1:pc {
    (text | model.gLike | c)*,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    
    ## indicates the extent to which this punctuation mark
    ##       conventionally separates words or phrases
    attribute force {
      
      ## the punctuation mark is a word separator
      "strong"
      | 
        ## the punctuation mark is not a word separator
        "weak"
      | 
        ## the punctuation mark may or may not be a
        ## word separator
        "inter"
    }?,
    
    ## provides a name for the kind of unit delimited by  this punctuation mark.
    attribute unit { data.enumerated }?,
    
    ## indicates whether this punctuation mark precedes or
    ##       follows the unit it delimits.
    attribute pre { data.truthValue }?,
    empty
  }
att.global.analytic.attributes = att.global.analytic.attribute.ana
att.global.analytic.attribute.ana =
  
  ##  (analysis) 指出一個或多個元素，其中包含帶有屬性ana的元素解釋。
  attribute ana {
    list { data.pointer, data.pointer* }
  }?
span =
  
  ## 將詮釋性註釋直接和一段文字段連結。
  element ns1:span {
    macro.phraseSeq.limited,
    att.global.attributes,
    att.interpLike.attributes,
    
    ## 指出被註解段落的起點；若沒有使用屬性to，則指出全部段落。
    attribute from { data.pointer },
    
    ## 指出被註解段落的終點。
    attribute to { data.pointer }?,
    empty
  }
spanGrp =
  
  ##  (文字段群組) 匯集文字段標籤。
  element ns1:spanGrp {
    span*, att.global.attributes, att.interpLike.attributes, empty
  }
interp =
  
  ##  (解釋) 標明和某一文字段相連結的特定解釋性註釋。
  element ns1:interp {
    (text | model.gLike | model.glossLike)*,
    att.global.attributes,
    att.interpLike.attributes,
    empty
  }
interpGrp =
  
  ##  (解釋群組) 匯集一系列具有共同任務或類型的相關解釋。
  element ns1:interpGrp {
    (model.glossLike*, interp+),
    att.global.attributes,
    att.interpLike.attributes,
    empty
  }
precision =
  
  ## indicates the numerical accuracy or precision  associated
  ##   with some aspect of the text markup. 
  element ns1:precision {
    model.glossLike*,
    att.global.attributes,
    att.scoping.attributes,
    
    ## indicates the degree of precision to be assigned as a
    ##       value between 0 (none) and 1 (optimally precise)
    attribute degree { data.probability }?,
    
    ## supplies a standard deviation associated with the value in
    ##       question
    attribute stdDeviation { data.numeric }?,
    empty
  }
certainty =
  
  ## 指出標記文本某些部分的確定或不確定的程度。
  element ns1:certainty {
    model.glossLike*,
    att.global.attributes,
    att.scoping.attributes,
    
    ## 指出不確定標記的確切位置：元素的應用性、開始標籤或結束標籤的確切位置、特定屬性的屬性值等。
    attribute locus {
      
      ## uncertainty concerns whether the name of the element
      ##	  or attribute  used is correctly applied.
      "name"
      | 
        ## uncertainty concerns whether the start of the element
        ##	  is correctly identified.
        "start"
      | 
        ## uncertainty concerns whether the end of the element
        ##	  is correctly identified.
        "end"
      | 
        ## uncertainty concerns both the start and the end of the
        ##	  element.
        "location"
      | 
        ## uncertainty concerns the content (for an element) or
        ##	  the value (for an attribute)
        "value"
    },
    
    ## 提供一個替代屬性值給指定標記部分—一個替代的類目識別符碼、轉錄、或屬性值，或是元素anchor (用來指出一個替代的開始或結束位置) 的識別符碼。若使用屬性assertedValue，則屬性degree中指出的確定程度適用於屬性assertedValue所指明的替代標記；若無使用，則確定程度適用於文本中的標記。
    attribute assertedValue { data.pointer | data.name | data.word }?,
    
    ## 指出在評估確定程度時所設想的情況。
    attribute given {
      list { data.pointer, data.pointer* }
    }?,
    
    ## 指出屬性locus所指定的標記部分之確定程度。
    attribute degree { data.probability }?,
    empty
  }
respons =
  
  ##  (責任) 指出標記一或多個特定元素某部分的負責人。
  element ns1:respons {
    model.glossLike*,
    att.global.attributes,
    att.scoping.attributes,
    
    ## 指出標記責任所屬的特定部分。
    attribute locus {
      list {
        (
         ## responsibility is being assigned concerning the name of the element
         ##	  or attribute used.
         "name"
         | 
           ## responsibility is being assigned concerning the start of the element
           ##	  concerned.
           "start"
         | 
           ## responsibility is being assigned concerning the end of the element
           ##	  concerned.
           "end"
         | 
           ## responsibility is being assigned concerning the location of the element
           ##	  concerned.
           "location"
         | 
           ## responsibility is being assigned concerning the content (for an element) or
           ##	  the value (for an attribute)
           "value"),
        (
         ## responsibility is being assigned concerning the name of the element
         ##	  or attribute used.
         "name"
         | 
           ## responsibility is being assigned concerning the start of the element
           ##	  concerned.
           "start"
         | 
           ## responsibility is being assigned concerning the end of the element
           ##	  concerned.
           "end"
         | 
           ## responsibility is being assigned concerning the location of the element
           ##	  concerned.
           "location"
         | 
           ## responsibility is being assigned concerning the content (for an element) or
           ##	  the value (for an attribute)
           "value")*
      }
    },
    
    ##  (responsible party) 指出負責電子文本指定部分的個人或機構。
    attribute resp { data.pointer },
    empty
  }
g =
  
  ##  (文字或字體) 非標準的文字或字體。
  element ns1:g {
    text,
    att.global.attributes,
    att.typed.attributes,
    
    ## 連結到該文字或字體的描述。
    attribute ref { data.pointer }?,
    empty
  }
char =
  
  ##  (文字) 提供一個文字的描述性資訊。
  element ns1:char {
    (charName?,
     model.glossLike*,
     charProp*,
     mapping*,
     empty*,
     model.graphicLike*,
     model.noteLike*),
    att.global.attributes,
    empty
  }
charName =
  
  ##  (文字名稱) 包含一文字名稱，其表示法根據統一碼協定 (Unicode)。
  element ns1:charName { text, att.global.attributes, empty }
charProp =
  
  ##  (文字性質) 提供父文字或字體的性質名稱與性質值。
  element ns1:charProp {
    ((unicodeName | localName), value),
    att.global.attributes,
    att.typed.attributes,
    empty
  }
charDecl =
  
  ##  (文字描述) 提供文字或字體的描述性資訊。
  element ns1:charDecl {
    (desc?, (char | glyph)+),
    att.global.attributes,
    empty
  }
glyph =
  
  ##  (文字的形體) 提供一種字體的描述性資訊。
  element ns1:glyph {
    (glyphName?,
     model.glossLike*,
     charProp*,
     mapping*,
     empty*,
     model.graphicLike*,
     model.noteLike*),
    att.global.attributes,
    empty
  }
glyphName =
  
  ##  (字體名稱) 包含一字體名稱，其表示法根據統一碼協定 (Unicode)。
  element ns1:glyphName { text, att.global.attributes, empty }
localName =
  
  ##  (當地定義的性質名稱) 包含某性質的地方性定義名稱。
  element ns1:localName { text, att.global.attributes, empty }
mapping =
  
  ##  (文字對應) 包含與父文字或字體在某方面有所關聯的一個或多個文字，在屬性type中加以說明。
  element ns1:mapping {
    macro.xtext, att.global.attributes, att.typed.attributes, empty
  }
unicodeName =
  
  ##  (統一碼性質名稱) 包含一個統一碼規範性質之註冊名稱。
  element ns1:unicodeName {
    text,
    att.global.attributes,
    
    ## 說明該性質名稱是定義在哪一版本的統一碼標準 (Unicode Standard) 中。
    attribute version { data.version }?,
    empty
  }
value =
  
  ##  (性質值) 包含一個性質值、屬性值、或其他分析值。
  element ns1:value { macro.xtext, att.global.attributes, empty }
model.persNamePart =
  surname | forename | genName | nameLink | addName | roleName
model.persNamePart_alternation =
  surname | forename | genName | nameLink | addName | roleName
model.persNamePart_sequence =
  surname, forename, genName, nameLink, addName, roleName
model.persNamePart_sequenceOptional =
  surname?, forename?, genName?, nameLink?, addName?, roleName?
model.persNamePart_sequenceOptionalRepeatable =
  surname*, forename*, genName*, nameLink*, addName*, roleName*
model.persNamePart_sequenceRepeatable =
  surname+, forename+, genName+, nameLink+, addName+, roleName+
orgName =
  
  ##  (組織名稱) 包含一個組織名稱。
  element ns1:orgName {
    macro.phraseSeq,
    att.global.attributes,
    att.datable.attributes,
    att.editLike.attributes,
    att.personal.attributes,
    att.typed.attributes,
    empty
  }
persName =
  
  ##  (個人名稱) 包含一個人名，可能是任何或全部屬於該人物的名字、姓氏、敬稱、或附加名等。
  element ns1:persName {
    macro.phraseSeq,
    att.global.attribute.xmlid,
    att.global.attribute.n,
    att.global.attribute.xmllang,
    att.global.attribute.rend,
    att.global.attribute.rendition,
    att.global.attribute.xmlbase,
    att.global.attribute.xmlspace,
    
    ##  (corresponds) Key for the authority database.
    attribute corresp {
      xsd:string { pattern = "(A\d{6})|(G\d{6})" }
    }?,
    att.global.linking.attribute.synch,
    att.global.linking.attribute.sameAs,
    att.global.linking.attribute.copyOf,
    att.global.linking.attribute.next,
    att.global.linking.attribute.prev,
    att.global.linking.attribute.exclude,
    att.global.linking.attribute.select,
    
    ##  (analysis) Allows a value restricted by a regEx.
    attribute ana {
      xsd:string { pattern = "#person" }
    }?,
    att.datable.attributes,
    att.editLike.attributes,
    att.personal.attribute.full,
    att.personal.attribute.sort,
    att.naming.attribute.role,
    att.naming.attribute.nymRef,
    
    ## Key for the authority database.
    attribute key {
      xsd:string { pattern = "(A\d{6})|(G\d{6})|unknown" }
    }?,
    att.canonical.attribute.ref,
    att.typed.attributes,
    empty
  }
surname =
  
  ## 包含一個家族姓氏，並非名字、教名、或綽號。
  element ns1:surname {
    macro.phraseSeq,
    att.global.attributes,
    att.personal.attributes,
    att.typed.attributes,
    empty
  }
forename =
  
  ## 包含名字或教名。
  element ns1:forename {
    macro.phraseSeq,
    att.global.attributes,
    att.personal.attributes,
    att.typed.attributes,
    empty
  }
genName =
  
  ## 該名稱用來和其他相似名稱做區別，以個人的相對年紀或隸屬世代作依據。
  element ns1:genName {
    macro.phraseSeq,
    att.global.attributes,
    att.personal.attributes,
    att.typed.attributes,
    empty
  }
nameLink =
  
  ##  (name link) 包含一個名稱中的連結字彙，但不屬於該名稱的一部份，例如 van der 或是 of。
  element ns1:nameLink {
