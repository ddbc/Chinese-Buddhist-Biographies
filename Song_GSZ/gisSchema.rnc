model.rdgLike = rdg
app =
  
  ##  (學術編輯註解項目) 一項學術編輯註解，內含一個不必備主題以及至少一個對應本。
  element ns1:app {
    (model.global*,
     (lem, model.global*, (wit, model.global*)?)?,
     ((model.rdgLike, model.global*, (wit, model.global*)?)
      | (rdgGrp, model.global*, (wit, model.global*)?))*)
    >> sch:pattern [
         id = "app-constraint-only1lem"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:app"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "count( descendant::tei:lem[ generate-id(       current() ) = generate-id( ancestor::tei:app[1] ) ]) <       2"
             "Only one <lem> element may appear within a single\x{a}" ~
             "      apparatus entry, whether it appears outside a <rdgGrp>\x{a}" ~
             "      element or within it."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    
    ## 利用合適的分類法將此元素所標記的變異分類。
    attribute type { data.enumerated }?,
    
    ## 必要情況下，指出基礎文件中該主題的開端。
    attribute from { data.pointer }?,
    
    ## 必要情況下，指出基礎文件中該主題的結尾。
    attribute to { data.pointer }?,
    
    ##  (location) 使用學術編輯註解標記的位置參照辦法時，指出變異的位置。
    attribute loc {
      list { data.word, data.word* }
    }?,
    empty
  }
lem =
  
  ##  (主題) 包含一個原文變異的主題或基礎文件。
  element ns1:lem {
    (text
     | model.gLike
     | model.phrase
     | model.inter
     | model.global
     | model.rdgPart)*,
    att.global.attributes,
    att.textCritical.attributes,
    empty
  }
rdg =
  
  ##  (對應本) 包含原文變異中的一個對應本。
  element ns1:rdg {
    (text
     | model.gLike
     | model.phrase
     | model.inter
     | model.global
     | model.rdgPart)*,
    att.global.attributes,
    att.textCritical.attributes,
    empty
  }
rdgGrp =
  
  ##  (對應本群組) 原文變異中，匯集兩個或多個認為具有根源關係或於其他方面性質類似的對應本。
  element ns1:rdgGrp {
    ((rdgGrp, wit?)
     | ((lem, wit?)?, (model.rdgLike, wit?))*)+,
    att.global.attributes,
    att.textCritical.attributes,
    empty
  }
witDetail =
  
  ## 進一步提供一個特殊對應本的特殊版本資訊。
  element ns1:witDetail {
    macro.phraseSeq,
    att.global.attributes,
    att.placement.attributes,
    att.responsibility.attributes,
    att.pointing.attributes,
    
    ##  (witnesses) 標明細節資訊所指版本的一個或多個印記。
    attribute wit {
      list { data.pointer, data.pointer* }
    },
    
    ## 版本的資訊種類。
    attribute type { data.enumerated }?,
    empty
  }
wit =
  
  ## 原文變異中，表明已知對應本版本的一個或多個印記的列表。
  element ns1:wit {
    macro.phraseSeq,
    att.global.attributes,
    att.rdgPart.attributes,
    empty
  }
listWit =
  
  ##  (witness list) lists definitions for all the witnesses referred to by a critical
  ##   apparatus, optionally grouped hierarchically.
  element ns1:listWit {
    (model.headLike?, (witness | listWit)+),
    att.global.attributes,
    empty
  }
witness =
  
  ## 包含學術編輯註解中所指的單一版本描述，或由一個印記所參照的版本列表。
  element ns1:witness {
    macro.limitedContent, att.global.attributes, empty
  }
witStart =
  
  ## 指明零散版本文字的開端或重新開始。
  element ns1:witStart {
    empty, att.global.attributes, att.rdgPart.attributes, empty
  }
witEnd =
  
  ## 指明零散版本文字的結尾或中止。
  element ns1:witEnd {
    empty, att.global.attributes, att.rdgPart.attributes, empty
  }
lacunaStart =
  
  ## 指出於大致完整的文字版本中，一個缺文的開始。
  element ns1:lacunaStart {
    empty, att.global.attributes, att.rdgPart.attributes, empty
  }
lacunaEnd =
  
  ## 指出於大致完整的文字版本中，一個缺文的結束。
  element ns1:lacunaEnd {
    empty, att.global.attributes, att.rdgPart.attributes, empty
  }
variantEncoding =
  
  ## 宣告在標記不同版本中的變異字體時所使用的方法。
  element ns1:variantEncoding {
    empty,
    att.global.attributes,
    
    ## 指明標記變異的學術編輯註解時所使用的方法。
    attribute method {
      
      ## 學術編輯註解使用基礎文件中被參照的行數或其他標準參照架構。
      "location-referenced"
      | 
        ## 學術編輯註解指出與基礎文件相關的各主題起始與結束的確切位置
        "double-end-point"
      | 
        ## 文件中一個段落的多個替代讀本以平行方式呈現；不需要基礎文件
        "parallel-segmentation"
    },
    
    ## 指出該學術編輯註解出現在連續文字內部或其外部。
    attribute location {
      
      ## 學術編輯註解出現在連續文字內部
      "internal"
      | 
        ## 學術編輯註解出現在基礎文件外部
        "external"
    },
    empty
  }
model.rdgPart = wit | witStart | witEnd | lacunaStart | lacunaEnd
att.rdgPart.attributes = att.rdgPart.attribute.wit
att.rdgPart.attribute.wit =
  
  ##  (witness or witnesses) 一個或多個印記列表，指出於此處開始或結束的版本。
  attribute wit {
    list { data.pointer, data.pointer* }
  }?
att.textCritical.attributes =
  att.responsibility.attributes,
  att.textCritical.attribute.wit,
  att.textCritical.attribute.type,
  att.textCritical.attribute.cause,
  att.textCritical.attribute.varSeq,
  att.textCritical.attribute.hand
att.textCritical.attribute.wit =
  
  ##  (witness or witnesses) 包含一個或多個指標的一份列表，連結到已知對應本的版本。
  attribute wit {
    list { data.pointer, data.pointer* }
  }?
att.textCritical.attribute.type =
  
  ## 用合宜的分類法將對應本分類。
  ## 實例值包含: 1] substantive; 2] orthographic
  attribute type { data.enumerated }?
att.textCritical.attribute.cause =
  
  ## 利用可能來源的任何適當分類法，將變異對應本的產生原因分類。
  ## 實例值包含: 1] homeoteleuton; 2] homeoarchy; 3] paleographicConfusion; 4] haplography; 5] dittography; 6] falseEmendation
  attribute cause { data.enumerated }?
att.textCritical.attribute.varSeq =
  
  ##  (variant sequence) 當有理由假設任一主題的一系列變異對應本時，則提供一個數字，指出此對應本於連續系列中的位置。
  attribute varSeq { data.count }?
att.textCritical.attribute.hand =
  
  ## 指出該版本中特定對應本的負責人。
  attribute hand { data.pointer }?
link =
  
  ## 定義元素或段落之間的關連或超文字連結，其他元素無法將此連結類型作更詳細說明。
  element ns1:link {
    empty
    >> sch:pattern [
         id = "link-constraint-linkTargets1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @targets"
             "You may not supply both \x{a}" ~
             "	@target and @targets"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "link-constraint-linkTargets2"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:report [
             test = "not(@target) and not(@targets)"
             "You must\x{a}" ~
             "	supply either @target or @targets"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "link-constraint-linkTargets3"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "contains(@target,' ')"
             "You must supply at least two\x{a}" ~
             "values for @target"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    att.pointing.attributes,
    att.typed.attributes,
    
    ## 標明被連結元素或段落的識別符碼。
    attribute targets {
      list { data.pointer, data.pointer, data.pointer* }
    }?,
    empty
  }
linkGrp =
  
  ## 定義一群關連或超文字連結的集合。
  element ns1:linkGrp {
    (link | ptr | note)+,
    att.global.attributes,
    att.pointing.group.attributes,
    empty
  }
ab =
  
  ##  (匿名區塊) 包含文件中任何隨機組合性層次的單元，匿名收容類似一個段落、但不包含段落語義的字詞或中間層元素。
  element ns1:ab {
    macro.paraContent,
    att.global.attributes,
    att.typed.attributes,
    att.declaring.attributes,
    
    ## 標明區塊是否完全。
    [ a:defaultValue = "N" ]
    attribute part {
      
      ##  (yes) 區塊不完全
      "Y"
      | 
        ##  (no) 區塊完全，或完全程度不明
        "N"
      | 
        ##  (initial) 不完全區塊的起始部分
        "I"
      | 
        ##  (medial) 不完全區塊的中段部分
        "M"
      | 
        ##  (final) 不完全區塊的末端部分
        "F"
    }?,
    empty
  }
anchor =
  
  ##  (錨點) 在文件中某一位置附上識別符碼，無論該位置是否符合一個文字性元素。
  element ns1:anchor {
    empty, att.global.attributes, att.typed.attributes, empty
  }
seg =
  
  ##  (隨機分割) 包含文件中任何隨機字詞層次的單元 (包括其他分割元素)。
  element ns1:seg {
    macro.paraContent,
    att.global.attributes,
    att.segLike.attributes,
    att.typed.attributes,
    att.responsibility.attributes,
    empty
  }
when =
  
  ## 指出一個時間點，相對或絕對於同一時間進程標籤裡的其他元素。
  element ns1:when {
    empty,
    att.global.attributes,
    
    ## 提供一個絕對時間值。
    attribute absolute { data.temporal.w3c }?,
    
    ## 若未由父元素timeLine得到時間單位資訊，則在此指明屬性interval所使用的時間單位。
    ## 被推薦的值包含: 1] d (days) ; 2] h (hours) ; 3] min (minutes) ; 4] s (seconds) ; 5] ms (milliseconds) 
    attribute unit {
      
      ##  (days) 
      "d"
      | 
        ##  (hours) 
        "h"
      | 
        ##  (minutes) 
        "min"
      | 
        ##  (seconds) 
        "s"
      | 
        ##  (milliseconds) 
        "ms"
      | xsd:Name
    }?,
    
    ## 指明時間區段中的數值部份
    attribute interval {
      xsd:float { minExclusive = "0" }
      | "unknown"
    }?,
    
    ## 指出一個參照點，用以確定現有元素when的時間，可於參照點的時間內加入區段而取得。
    attribute since { data.pointer }?,
    empty
  }
timeline =
  
  ##  (時間進程) 提供一組整齊的時間順序點，可與口說文本的元素相連結，以建立該文本的時間組序。
  element ns1:timeline {
    when+,
    att.global.attributes,
    
    ## 標出時間進程的開頭，例如開始時間。
    attribute origin { data.pointer }?,
    
    ## 指出符合時間進程屬性interval的屬性值或是符合其接續時間點的時間單位。
    ## 被推薦的值包含: 1] d (days) ; 2] h (hours) ; 3] min (minutes) ; 4] s (seconds) ; 5] ms (milliseconds) 
    attribute unit {
      
      ##  (days) 
      "d"
      | 
        ##  (hours) 
        "h"
      | 
        ##  (minutes) 
        "min"
      | 
        ##  (seconds) 
        "s"
      | 
        ##  (milliseconds) 
        "ms"
      | xsd:Name
    }?,
    
    ## 指明時間區段中的數值部份
    attribute interval {
      xsd:float { minExclusive = "0" }
      | "regular"
      | "irregular"
    }?,
    empty
  }
join =
  
  ## 標明文本中可能的不完整分割，指向構成此不完整分割的可能不連接元素。
  element ns1:join {
    (model.glossLike*)
    >> sch:pattern [
         id = "join-constraint-joinTargets1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:join"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @targets"
             "You may not supply both \x{a}" ~
             "	@target and @targets"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "join-constraint-joinTargets2"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:join"
           "\x{a}" ~
           "               "
           sch:report [
             test = "not(@target) and not(@targets)"
             "You must\x{a}" ~
             "	supply either @target or @targets"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "join-constraint-joinTargets3"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:join"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "contains(@target,' ')"
             "You must supply at least two\x{a}" ~
             "values for @target"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attributes,
    att.pointing.attributes,
    att.typed.attributes,
    
    ## 標明與一個虛擬元素相連結的元素或段落識別符碼。
    attribute targets {
      list { data.pointer, data.pointer, data.pointer* }
    }?,
    
    ## 指出一個推斷該元素集合所代表的元素名稱。
    attribute result { data.name }?,
    
    ## 指出被連結的目標是否包含所指元素的完整部份 (完整樹狀圖包含根節點) ，或僅包含該目標的子元素 (樹狀分支部份) 。
    [ a:defaultValue = "root" ]
    attribute scope {
      
      ## 由屬性target所指明、具根節點的樹狀圖彼此連結，每個樹狀圖成為該連結所產生之虛擬元素的子元素。
      "root"
      | 
        ## 由屬性target所指明的樹狀圖的子元素成為虛擬元素的子元素 (例如捨去樹狀圖的根節點) 。
        "branches"
    }?,
    empty
  }
joinGrp =
  
  ##  (連結群組) 匯集一群連結元素與可能指標。
  element ns1:joinGrp {
    (model.glossLike*, (join | ptr)+),
    att.global.attributes,
    att.pointing.group.attributes,
    
    ## 描述此集合中匯集的連結結果。
    attribute result { data.name }?,
    empty
  }
att.global.linking.attributes =
  att.global.linking.attribute.corresp,
  att.global.linking.attribute.synch,
  att.global.linking.attribute.sameAs,
  att.global.linking.attribute.copyOf,
  att.global.linking.attribute.next,
  att.global.linking.attribute.prev,
  att.global.linking.attribute.exclude,
  att.global.linking.attribute.select
att.global.linking.attribute.corresp =
  
  ##  (corresponds) 連結到的元素在某方面符合現有元素。
  attribute corresp {
    list { data.pointer, data.pointer* }
  }?
att.global.linking.attribute.synch =
  
  ##  (synchronous) 連結到的元素和現有元素同時出現。
  attribute synch {
    list { data.pointer, data.pointer* }
  }?
att.global.linking.attribute.sameAs =
  
  ## 連結到的元素和現有元素相同。
  attribute sameAs { data.pointer }?
att.global.linking.attribute.copyOf =
  
  ## 現有元素為所連結元素的複製本。
  attribute copyOf { data.pointer }?
att.global.linking.attribute.next =
  
  ## 連結到現有元素所屬虛擬集合中的下一個元素。
  attribute next { data.pointer }?
att.global.linking.attribute.prev =
  
  ##  (previous) 連結到現有元素所屬虛擬集合中的上一個元素。
  attribute prev { data.pointer }?
att.global.linking.attribute.exclude =
  
  ## 連結到的元素是現有元素的專有替換元素。
  attribute exclude {
    list { data.pointer, data.pointer* }
  }?
att.global.linking.attribute.select =
  
  ## 選擇一個或多個替換；若選擇一個替換，則疑惑或不明確被標記為已解決。若選擇的替換多於一個，則疑惑或不明確的程度因為未選擇的替換而被標記為減低。
  attribute select {
    list { data.pointer, data.pointer* }
  }?
textDesc =
  
  ##  (文本描述) 提供文本在情境特徵方面的相關描述。
  element ns1:textDesc {
    (model.textDescPart_sequence, purpose+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
particDesc =
  
  ##  (參與描述) 描述在一個語言互動中可辨識的說話者、聲音或其他參與者。
  element ns1:particDesc {
    (model.pLike+ | (model.personLike | listPerson)+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
settingDesc =
  
  ##  (背景描述) 描述語言互動所發生的一個或多個背景，可用一段散文描述或是一系列的背景元素來表達。
  element ns1:settingDesc {
    (model.pLike+ | (setting | model.placeLike | listPlace)+),
    att.global.attributes,
    att.declarable.attributes,
    empty
  }
channel =
  
  ##  (主要途徑) 描述文本傳達的媒介或途徑。書面文本的途徑可能是印製、手稿、寫本、電子郵件等；口說文本則有廣播、電話、面對面交談等。
  element ns1:channel {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明該途徑為口說或書面模式。
    [ a:defaultValue = "x" ]
    attribute mode {
      
      ##  (spoken) 
      "s"
      | 
        ##  (written) 
        "w"
      | 
        ##  (spoken to be written) 為寫而說 (例如聽寫) 　
        "sw"
      | 
        ##  (written to be spoken) 為說而寫 (例如稿子) 　
        "ws"
      | 
        ##  (mixed) 
        "m"
      | 
        ##  (unknown or inapplicable) 
        "x"
    }?,
    empty
  }
constitution =
  
  ## 描述文本或文本樣本的內部結構，例如零碎的或完整的結構。
  element ns1:constitution {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明文本構成方式。
    [ a:defaultValue = "single" ]
    attribute type {
      
      ## 單一完整的文本
      "single"
      | 
        ## 文本由許多小項目所組成，每個個別項目都是完整的
        "composite"
      | 
        ##  (fragments) 文本由許多小項目所組成，每個項目不一定完整
        "frags"
      | 
        ## 文本結構未知或未加以說明
        "unknown"
    }?,
    empty
  }
derivation =
  
  ## 描述文本的特性與原創性的程度。
  element ns1:derivation {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 將文本來歷分類。
    ## 實例值包含: 1] original; 2] revision; 3] translation; 4] abridgment; 5] plagiarism; 6] traditional
    attribute type { data.enumerated }?,
    empty
  }
domain =
  
  ##  (應用領域) 描述該文本最重要的應用領域或是社會用途所在，例如私人與公共、教育、宗教等。
  element ns1:domain {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 將應用領域分類。
    ## 實例值包含: 1] art; 2] domestic; 3] religious; 4] business; 5] education; 6] govt (government) ; 7] public
    attribute type { data.enumerated }?,
    empty
  }
factuality =
  
  ## 描述該文本內容可能被認定為想像的程度、所描繪的為虛構或真實的世界。
  element ns1:factuality {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 將文本的真實性分類。
    attribute type {
      
      ## 文本為完全虛構
      "fiction"
      | 
        ## 文本完全根據事實
        "fact"
      | 
        ## 文本內容部分虛構、部分根據事實
        "mixed"
      | 
        ## 該文本不適宜使用虛構／真實的區分方法
        "inapplicable"
    }?,
    empty
  }
interaction =
  
  ## 描述文本創作者與體驗者之間互動關係的規範、基數和特性，例如回應或感嘆、評論等。
  element ns1:interaction {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明文本中主動與被動參與者之間的互動程度。
    attribute type {
      
      ## 無任何互動，例如獨白
      "none"
      | 
        ## 某種程度上的互動，例如具有背景回應的獨白
        "partial"
      | 
        ## 完全性的互動，例如面對面交談
        "complete"
      | 
        ## 這項特性在此不適當或不適用
        "inapplicable"
    }?,
    
    ## 說明創作文本部份內容的主動參與者 (或傳達者) 的人數。
    ## 被推薦的值包含: 1] singular; 2] plural; 3] corporate; 4] unknown
    attribute active {
      
      ## 一位傳達者
      "singular"
      | 
        ## 多位傳達者
        "plural"
      | 
        ## 集體傳達者
        "corporate"
      | 
        ## 傳達人數未知或未指明
        "unknown"
      | xsd:Name
    }?,
    
    ## 說明文本內容的被動參與者 (或接收者) 的人數，被動參與者即為文本產生或呈現的對象。
    ## 被推薦的值包含: 1] self; 2] single; 3] many; 4] group; 5] world
    attribute passive {
      
      ## 文本內容傳遞給原創作者，例如日記
      "self"
      | 
        ## 文本內容傳遞給另一個人，例如私人信件
        "single"
      | 
        ## 文本內容傳遞給一群人數可計的對象，例如參與者身分皆明確的一場對話
        "many"
      | 
        ## 文本內容傳遞給一群身分不明確但人數固定的參與者，例如授課
        "group"
      | 
        ## 文本內容傳遞給眾多身分不明確、數量亦不固定的參與者，例如出版的書籍
        "world"
      | xsd:Name
    }?,
    empty
  }
preparedness =
  
  ## 描述文本可能被視為經過準備或自然呈現的程度。
  element ns1:preparedness {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明準備程度的類型。
    ## 實例值包含: 1] none; 2] scripted; 3] formulaic; 4] revised
    attribute type { data.enumerated }?,
    empty
  }
purpose =
  
  ## 描述該文本的一項目的或溝通功能
  element ns1:purpose {
    macro.phraseSeq.limited,
    att.global.attributes,
    
    ## 說明一項特定類型的目的。
    ## 被推薦的值包含: 1] persuade; 2] express; 3] inform; 4] entertain
    attribute type {
      
      ## 教導、廣告、宣傳等
      "persuade"
      | 
        ## 自我表達、自白等
        "express"
      | 
        ## 傳達訊息、教育等
        "inform"
      | 
        ## 消遣、娛樂等
        "entertain"
      | xsd:Name
    }?,
    
    ## 說明該目的所佔的重要性程度。
    attribute degree { data.certainty }?,
    empty
  }
setting =
  
  ## 描述語言互動發生的一個特定背景。
  element ns1:setting {
    (model.pLike+
     | (model.nameLike.agent | model.dateLike | model.settingPart)*),
    att.global.attributes,
    att.ascribed.attributes,
    empty
  }
locale =
  
  ## 包含簡短非正式的文字描述地點的性質，例如房間、餐廳、公園長椅等。
  element ns1:locale {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
activity =
  
  ## 包含簡短非正式的文字，描述一個對話參與者除了說話以外其他可能的動作。
  element ns1:activity {
    macro.phraseSeq.limited, att.global.attributes, empty
  }
start = TEI | teiCorpus
